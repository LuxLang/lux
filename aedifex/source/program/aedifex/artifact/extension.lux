(.using
 [library
  [lux (.except type)
   [data
    ["[0]" text (.only)
     ["%" \\injection]]]
   [macro
    ["[0]" template]]]]
 ["[0]" //
  ["[1]" type]])

(.every .public Extension
  Text)

... https://en.wikipedia.org/wiki/Delimiter
(the delimiter
  ".")

(the .public extension
  (-> //.Type Extension)
  (|>> (text ..delimiter)))

(the .public type
  (-> Extension //.Type)
  (text.replaced ..delimiter ""))

(template.with [<name>]
  [(the .public <name>
     Extension
     (..extension (template.name [//._] [<name>])))]

  [[lux_library]
   [jvm_library]
   [js_library]
   [pom]
   [sha1]
   [md5]])
