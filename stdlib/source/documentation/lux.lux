    [control
     ["." io]]]]
  [\\library
   ["." /]]
  ["." / #_
   ["#." abstract]
   ["#." control]
   ["#." data]
   ["#." debug]
   ["#." documentation]
   ["#." ffi]
   ["#." locale]
   ["#." macro]
