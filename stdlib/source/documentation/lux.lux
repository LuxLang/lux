   ])

(.def: .public documentation
  (.List $.Module)
  ($.module /._
            ""
            []
            [/abstract.documentation
             /control.documentation
             /data.documentation
             /debug.documentation
             /documentation.documentation
             /ffi.documentation
             /locale.documentation
             /macro.documentation

             /program.documentation]))

(program: inputs
  (io.io (debug.log! ($.documentation ..documentation))))
