(.module:
  [library
   [lux #*
    ["$" documentation (#+ documentation:)]
    [data
     [text (#+ \n)
      ["%" format (#+ format)]]
     [collection
      ["." list]]]]]
  ["." / #_
   ["#." binary]
   ["#." bit]
   ["#." collection]
   ["#." color]
   ["#." format]
   ["#." identity]
   ["#." name]
   ["#." product]
   ["#." sum]
   ["#." text]])

(.def: .public documentation
  (.List $.Module)
  (list.together
   (list /binary.documentation
         /bit.documentation
         /collection.documentation
         /color.documentation
         /format.documentation
         /identity.documentation
         /name.documentation
         /product.documentation
         /sum.documentation
         /text.documentation
         )))
