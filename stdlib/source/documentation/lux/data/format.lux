(.module:
  [library
   [lux "*"
    ["$" documentation {"+" [documentation:]}]
    [data
     [text {"+" [\n]}
      ["%" format {"+" [format]}]]
     [collection
      ["." list]]]]]
  ["." / "_"
   ["#." binary]
   ["#." json]
   ["#." tar]
   ["#." xml]])

(.def: .public documentation
  (.List $.Module)
  (list.together
   (list /binary.documentation
         /json.documentation
         /tar.documentation
         /xml.documentation
         )))
