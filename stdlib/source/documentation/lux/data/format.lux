(.require
 [library
  [lux (.except)
   ["$" documentation (.only documentation:)]
   [data
    [text (.only \n)
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 ["[0]" /
  ["[1][0]" binary]
  ["[1][0]" json]
  ["[1][0]" tar]
  ["[1][0]" xml]])

(.def .public documentation
  (.List $.Module)
  (list.together
   (list /binary.documentation
         /json.documentation
         /tar.documentation
         /xml.documentation
         )))
