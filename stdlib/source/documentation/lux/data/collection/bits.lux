(.module:
  [library
   [lux {"-" [or and not]}
    ["$" documentation {"+" [documentation:]}]
    [control
     ["<>" parser
      ["<.>" code]]]
    [data
     ["." text {"+" [\n]}
      ["%" format {"+" [format]}]]]
    [macro
     [syntax {"+" [syntax:]}]
     ["." code]
     ["." template]]]]
  [\\library
   ["." /]])

(documentation: /.Bits
  "A bit-map.")

(documentation: /.size
  "Measures the size of a bit-map by counting all the 1s in the bit-map."
  [(size bits)])

(documentation: /.capacity
  ""
  [(capacity bits)])

(documentation: /.bit
  ""
  [(bit index bits)])

(documentation: /.intersects?
  ""
  [(intersects? reference sample)])

(documentation: /.not
  ""
  [(not input)])

(.def: .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [..Bits
             ..size
             ..capacity
             ..bit
             
             ..intersects?
             ..not
             
             ($.default /.Chunk)
             ($.default /.chunk_size)
             ($.default /.empty)
             ($.default /.empty?)
             ($.default /.equivalence)
             ($.default /.one)
             ($.default /.zero)
             ($.default /.flipped)
             ($.default /.and)
             ($.default /.or)
             ($.default /.xor)]
            []))
