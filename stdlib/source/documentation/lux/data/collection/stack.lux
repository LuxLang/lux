(.module:
  [library
   [lux {"-" [list]}
    ["$" documentation {"+" [documentation:]}]
    [control
     ["<>" parser
      ["<.>" code]]]
    [data
     ["." text {"+" [\n]}
      ["%" format {"+" [format]}]]]
    [macro
     ["." template]]]]
  [\\library
   ["." /]])

(documentation: (/.Stack it)
  "A first-in, last-out sequential data-structure.")

(documentation: /.value
  "Yields the top value in the stack, if any."
  [(value stack)])

(documentation: /.next
  ""
  [(next stack)])

(documentation: /.top
  ""
  [(top value stack)])

(.def: .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [..Stack
             ..value
             ..next
             ..top
             ($.default /.empty)
             ($.default /.size)
             ($.default /.empty?)
             ($.default /.equivalence)
             ($.default /.functor)]
            []))
