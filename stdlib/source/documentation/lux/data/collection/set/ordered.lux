(.module:
  [library
   [lux (#- list)
    ["$" documentation (#+ documentation:)]
    [control
     ["<>" parser
      ["<.>" code]]]
    [data
     ["." text (#+ \n)
      ["%" format (#+ format)]]]
    [macro
     ["." template]]]]
  [\\library
   ["." /]])

(documentation: /.Set
  "A set with ordered entries.")

(documentation: /.member?
  ""
  [(member? set elem)])

(documentation: /.has
  ""
  [(has elem set)])

(documentation: /.lacks
  ""
  [(lacks elem set)])

(documentation: /.difference
  ""
  [(difference param subject)])

(documentation: /.sub?
  "Is 'sub' a sub-set of 'super'?"
  [(sub? super sub)])

(documentation: /.super?
  "Is 'super' a super-set of 'sub'?"
  [(super? sub super)])

(.def: .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [..Set
             ..member?
             ..has
             ..lacks
             ..difference
             ..sub?
             ..super?
             ($.default /.empty)
             ($.default /.min)
             ($.default /.max)
             ($.default /.size)
             ($.default /.empty?)
             ($.default /.list)
             ($.default /.of_list)
             ($.default /.union)
             ($.default /.intersection)
             ($.default /.equivalence)]
            []))
