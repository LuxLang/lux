(.require
 [library
  [lux (.except list has)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]]
   [macro
    ["[0]" template]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.default /.empty)
             ($.default /.min)
             ($.default /.max)
             ($.default /.size)
             ($.default /.empty?)
             ($.default /.list)
             ($.default /.of_list)
             ($.default /.union)
             ($.default /.intersection)
             ($.default /.equivalence)

             ($.documentation (/.Set it)
               "A set with ordered entries.")

             ($.documentation /.member?
               ""
               [(member? set elem)])

             ($.documentation /.has
               ""
               [(has elem set)])

             ($.documentation /.lacks
               ""
               [(lacks elem set)])

             ($.documentation /.difference
               ""
               [(difference param subject)])

             ($.documentation /.sub?
               "Is 'sub' a sub-set of 'super'?"
               [(sub? super sub)])

             ($.documentation /.super?
               "Is 'super' a super-set of 'sub'?"
               [(super? sub super)])]
            []))
