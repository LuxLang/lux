(.require
 [library
  [lux (.except list has)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list (.use "[1]#[0]" monoid)]]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" multi]
  ["[1][0]" ordered]])

(def .public documentation
  (List $.Documentation)
  (list.partial ($.module /._
                          "")

                ($.definition (/.Set it))
                ($.definition /.member_hash)
                ($.definition /.empty)
                ($.definition /.size)
                ($.definition /.lacks)
                ($.definition /.member?)
                ($.definition /.list)
                ($.definition /.union)
                ($.definition /.equivalence)
                ($.definition /.hash)
                ($.definition /.monoid)
                ($.definition /.empty?)
                ($.definition /.of_list)
                ($.definition /.predicate)

                ($.definition /.has
                  ""
                  ($.example (has elem set)))

                ($.definition /.difference
                  ""
                  ($.example (difference sub base)))

                ($.definition /.intersection
                  ""
                  ($.example (intersection filter base)))

                ($.definition /.sub?
                  ""
                  ($.example (sub? super sub)))

                ($.definition /.super?
                  ""
                  ($.example (super? sub super)))

                (all list#composite
                     /multi.documentation
                     /ordered.documentation
                     )
                ))
