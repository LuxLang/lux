(.module:
  [library
   [lux #*
    ["$" documentation (#+ documentation:)]
    [data
     [text (#+ \n)
      ["%" format (#+ format)]]
     [collection
      ["." list]]]]]
  ["." / #_
   ["#." buffer]
   ["#." encoding]
   ["#." escape]
   ["#." format]
   ["#." regex]
   ["#." unicode]])

(.def: .public documentation
  (.List $.Module)
  (list.together
   (list /buffer.documentation
         /encoding.documentation
         /escape.documentation
         /format.documentation
         /regex.documentation
         /unicode.documentation
         )))
