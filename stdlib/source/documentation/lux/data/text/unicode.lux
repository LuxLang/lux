(.using
 [library
  [lux "*"
   ["$" documentation (.only documentation:)]
   [data
    [text
     ["%" format (.only format)]]
    [collection
     ["[0]" list]]]]]
 ["[0]" / "_"
  ["[1][0]" block]
  ["[1][0]" set]])

(.def: .public documentation
  (.List $.Module)
  (list.together
   (list /block.documentation
         /set.documentation
         )))
