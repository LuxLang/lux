(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 ["[0]" /
  ["[1][0]" block]
  ["[1][0]" set]])

(def .public documentation
  (List $.Documentation)
  (list.together
   (list /block.documentation
         /set.documentation
         )))
