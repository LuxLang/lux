(.module:
  [library
   [lux "*"
    ["$" documentation {"+" [documentation:]}]
    [data
     [text {"+" [\n]}
      ["%" format {"+" [format]}]]
     [collection
      ["." list]]]]]
  ["." / "_"
   ["#." block]
   ["#." set]])

(.def: .public documentation
  (.List $.Module)
  (list.together
   (list /block.documentation
         /set.documentation
         )))
