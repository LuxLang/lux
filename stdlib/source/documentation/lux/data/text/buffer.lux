(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    [text (.only \n)
     ["%" \\format (.only format)]]]]]
 [\\library
  ["[0]" /]])

(def .public documentation
  (List $.Documentation)
  (list ($.module /._
                  "")

        ($.definition /.empty)
        ($.definition /.then)
        ($.definition /.size)
        ($.definition /.text)

        ($.definition /.Buffer
          "Immutable text buffer for efficient text concatenation.")
        ))
