(.module:
  [library
   [lux "*"
    ["$" documentation {"+" [documentation:]}]
    [data
     [text {"+" [\n]}
      ["%" format {"+" [format]}]]]
    [macro
     ["." template]]]]
  [\\library
   ["." /]])

(documentation: /.codec
  "A codec for binary encoding of text as UTF-8.")

(.def: .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [..codec]
            []))
