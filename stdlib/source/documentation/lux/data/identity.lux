(.require
 [library
  [lux (.except nat int rev list or and)
   ["$" documentation]
   [data
    [text (.only \n)
     ["%" \\format (.only format)]]]
   [macro
    ["[0]" template]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.default /.functor)
             ($.default /.apply)
             ($.default /.monad)
             ($.default /.comonad)

             ($.documentation (/.Identity it)
               "A value, as is, without any extra structure super-imposed on it.")]
            []))
