(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    [collection
     ["[0]" list (.use "[1]#[0]" monoid)]]]]]
 [\\library
  ["[0]" /]])

(the .public documentation
  (List $.Documentation)
  (let [number (list ($.definition /.not_a_small_number)
                     ($.definition /.small_limit)
                     ($.definition /.Small)
                     ($.definition /.small)
