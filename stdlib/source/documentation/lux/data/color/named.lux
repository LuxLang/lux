(.using
 [library
  [lux (.except)
   ["$" documentation (.only documentation:)]
   [control
    ["<>" parser (.only)
     ["<[0]>" code]]]
   [data
    ["[0]" text
     ["%" \\format (.only format)]]]
   [macro
    ["[0]" code]
    ["[0]" template]]
   [math
    [number
     ["[0]" nat ("hex#[0]" hex)]]]]]
 [\\library
  ["[0]" /
   ["/[1]" //]]])

(with_template [<name>]
  [(documentation: <name>
     (let [[red green blue] (//.rgb <name>)
           [_ name] (symbol <name>)]
       (format "R:" (hex#encoded red)
               " G:" (hex#encoded green)
               " B:" (hex#encoded blue)
               " | " (text.replaced "_" " " name))))]

