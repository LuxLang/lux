(.module:
  [library
   [lux (#- char)
    ["$" documentation (#+ documentation:)]
    [data
     [text
      ["%" format (#+ format)]]
     [collection
      ["." list]]]]]
  ["." / #_
   ["#." infix]
   ... ["#." modulus]
   ... ["#." modular]
   ... ["#." number]
   ["#." logic]]
  [\\library
   ["." /]])

(.def: .public documentation
  (.List $.Module)
  ($.module /._
            ""
            []
            [/infix.documentation
             ... /modulus.documentation
             ... /modular.documentation
             ... /number.documentation
             /logic.documentation]))
