(.using
 [library
  [lux {"-" char}
   ["$" documentation {"+" documentation:}]
   [data
    [text
     ["%" format {"+" format}]]
    [collection
     ["[0]" list]]]]]
 ["[0]" / "_"
  ["[1][0]" infix]
  ["[1][0]" modulus]
  ["[1][0]" modular]
  ["[1][0]" number]
  ["[1][0]" logic]
  ["[1][0]" random]]
