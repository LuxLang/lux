(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list (.use "[1]#[0]" monoid)]]]]]
 ["[0]" /
  ["[1][0]" infix]
  ["[1][0]" modulus]
  ["[1][0]" modular]
  ["[1][0]" number]
  ["[1][0]" logic]
  ["[1][0]" random]]
 [\\library
  ["[0]" /]])

(def .public documentation
  (List $.Documentation)
  (list.partial ($.module /._
                          "")

                (all list#composite
                     /infix.documentation
                     /modulus.documentation
                     /modular.documentation
                     /number.documentation
                     /logic.documentation
                     /random.documentation
                     )
                ))
