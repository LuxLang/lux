(.require
 [library
  [lux (.except char)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.default /.equivalence)
             ($.default /.format)

             ($.documentation /.Declaration
               "A declaration for either a constant or a function.")

             ($.documentation /.parser
               "A parser for declaration syntax."
               ["Such as:"
                quux
                (foo bar baz)])]
            []))
