(.require
 [library
  [lux (.except char)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.default /.equivalence)
             ($.default /.format)

             ($.documentation /.Input
               "The common typed-argument syntax used by many macros.")

             ($.documentation /.parser
               "Parser for the common typed-argument syntax used by many macros.")]
            []))
