(.module:
  [library
   [lux "*"
    ["$" documentation {"+" [documentation:]}]
    [data
     [text {"+" [\n]}
      ["%" format {"+" [format]}]]
     [collection
      ["." list]]]]]
  ["." / "_"
   ["#." capability]
   ["#." policy]])

(.def: .public documentation
  (.List $.Module)
  (list.together
   (list /policy.documentation
         /capability.documentation)))
