(.using
 [library
  [lux (.except)
   ["$" documentation (.only documentation:)]
   [data
    [text (.only \n)
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 ["[0]" /
  ["[1][0]" capability]
  ["[1][0]" policy]])

(.def .public documentation
  (.List $.Module)
  (list.together
   (list /policy.documentation
         /capability.documentation)))
