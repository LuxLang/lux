(.module:
  [library
   [lux "*"
    ["$" documentation {"+" [documentation:]}]
    [data
     [text {"+" [\n]}
      ["%" format {"+" [format]}]]
     [collection
      ["[0]" list]]]]]
  ["[0]" / "_"
   ["[1][0]" capability]
   ["[1][0]" policy]])

(.def: .public documentation
  (.List $.Module)
  (list.together
   (list /policy.documentation
         /capability.documentation)))
