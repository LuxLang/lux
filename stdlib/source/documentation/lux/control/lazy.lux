(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    [text (.only \n)
     ["%" \\format (.only format)]]]]]
 [\\library
  ["[0]" /]])

(the .public documentation
  (.List $.Documentation)
  (list ($.module /._
                  "")

        ($.definition /.value)
        ($.definition /.equivalence)
        ($.definition /.functor)
        ($.definition /.apply)
        ($.definition /.monad)

        ($.definition (/.Lazy it)
          (format "A value specified by an expression that is calculated only at the last moment possible."
                  \n "Afterwards, the value is cached for future reference."))

        ($.definition /.lazy
          "Specifies a lazy value by providing the expression that computes it."
          ($.example (' (lazy eager_computation))))
        ))
