(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]]]]
 [\\library
  ["[0]" /]])

(def .public documentation
  (List $.Documentation)
  (list ($.module /._
                  "")

        ($.definition /.wrong_type)
        ($.definition /.format)

        ($.definition /.Dynamic
          "A value coupled with its type, so it can be checked later.")

        ($.definition /.dynamic
          ""
          ($.example (is Dynamic
                         (dynamic 123))))

        ($.definition /.static
          ""
          ($.example (is (try.Try Nat)
                         (static Nat (dynamic 123)))))
        ))
