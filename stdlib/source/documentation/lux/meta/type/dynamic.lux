(.require
 [library
  [lux (.except static)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]]
   [macro
    ["[0]" template]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.default /.wrong_type)
             ($.default /.format)

             ($.documentation /.Dynamic
               "A value coupled with its type, so it can be checked later.")

             ($.documentation /.dynamic
               ""
               [(is Dynamic
                    (dynamic 123))])

             ($.documentation /.static
               ""
               [(is (try.Try Nat)
                    (static Nat (dynamic 123)))])]
            []))
