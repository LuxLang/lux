(.require
 [library
  [lux (.except and)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]]
   [meta
    [macro
     ["[0]" template]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.documentation (/.Co it)
               "A constraint for covariant types.")

             ($.documentation (/.Contra it)
               "A constraint for contravariant types.")

             ($.documentation (/.In it)
               "A constraint for invariant types.")]
            []))
