(.require
 [library
  [lux (.except char)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.definition /.equivalence)
             ($.definition /.format)
             ($.definition /.parser)

             ($.definition /.Check
               "A type annotation for an expression.")]
            []))
