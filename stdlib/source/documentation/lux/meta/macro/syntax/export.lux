(.require
 [library
  [lux (.except char)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            "Syntax for marking a definition as an export."
            [($.definition /.default_policy)

             ($.definition /.parser
               ""
               [(parser un_exported)])]
            []))
