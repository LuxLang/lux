(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 [\\library
  ["[0]" /]])

(def .public documentation
  (List $.Documentation)
  (list ($.module /._
                  "Syntax for marking a definition as an export.")

        ($.definition /.default_policy)

        ($.definition /.parser
          ""
          [(parser un_exported)])
        ))
