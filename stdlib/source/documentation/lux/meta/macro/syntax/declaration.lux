(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list]]]]]
 [\\library
  ["[0]" /]])

(def .public documentation
  (List $.Documentation)
  (list ($.module /._
                  "")

        ($.definition /.equivalence)
        ($.definition /.format)

        ($.definition /.Declaration
          "A declaration for either a constant or a function.")

        ($.definition /.parser
          "A parser for declaration syntax."

          ($.comment "Such as:")
          ($.example (' quux))
          ($.example (' (foo bar baz))))
        ))
