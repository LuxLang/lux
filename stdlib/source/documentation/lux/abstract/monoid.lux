(.using
 [library
  [lux
   ["$" documentation (.only documentation:)]
   [data
    [text (.only \n)
     ["%" \\format (.only format)]]]]]
 [\\library
  ["[0]" /]])

(documentation: (/.Monoid it)
  (format "A way to compose values."
          \n "Includes an identity value which does not alter any other value when combined with."))

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [..Monoid
             ($.default /.and)]
            []))
