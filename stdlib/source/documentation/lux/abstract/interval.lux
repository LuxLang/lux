(.require
 [library
  [lux
   ["$" documentation]
   [data
    [text (.only \n)
     ["%" \\format (.only format)]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.default /.between)
             ($.default /.inner?)
             ($.default /.outer?)
             ($.default /.singleton?)
             ($.default /.within?)
             ($.default /.starts_with?)
             ($.default /.ends_with?)
             ($.default /.precedes?)
             ($.default /.succeeds?)
             ($.default /.before?)
             ($.default /.after?)
             ($.default /.touches?)
             ($.default /.starts?)
             ($.default /.finishes?)
             ($.default /.equivalence)
             ($.default /.nested?)
             ($.default /.overlaps?)

             ($.definition (/.Interval it)
               "A representation of top and bottom boundaries for an ordered type.")

             ($.definition /.singleton
               "An interval where both top and bottom are the same value."
               [(singleton enum elem)])

             ($.definition /.borders?
               "Where a value is at the border of an interval.")

             ($.definition /.union
               "An interval that spans both predecessors.")

             ($.definition /.intersection
               "An interval spanned by both predecessors.")

             ($.definition /.complement
               "The inverse of an interval.")

             ($.definition /.meets?
               "Whether an interval meets another one on its bottom/lower side.")]
            []))
