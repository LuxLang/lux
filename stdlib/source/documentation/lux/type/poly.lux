(.require
 [library
  [lux (.except and)
   ["$" documentation]
   [abstract
    [\\specification
     ["$[0]" equivalence]
     ["$[0]" codec]]]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]]
   [macro
    ["[0]" template]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.default /.polytypic)

             ($.documentation /.code
               ""
               [(code env type)])]
            []))
