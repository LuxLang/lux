(.require
 [library
  [lux (.except)
   ["$" documentation]
   ["[0]" debug]
   [control
    ["[0]" io]]
   [data
    [collection
     ["[0]" list (.use "[1]#[0]" monoid)]]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" uri]
  ["[1][0]" http
   ["[1]/[0]" client]
   ["[1]/[0]" status]]])

(def .public documentation
  (List $.Documentation)
  (list.partial ($.module /._
                          "")

                ($.definition /.Location)

                ($.definition /.Address
                  "A TCP/IP address.")

                ($.definition /.Port
                  "A TCP/IP port.")

                ($.definition /.URL
                  "A Uniform Resource Locator.")

                (all list#composite
                     /uri.documentation
                     /http/client.documentation
                     /http/status.documentation)
                ))
