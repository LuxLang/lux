(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\injection (.only format)]]]]]
 [\\library
  ["[0]" /]])

(the .public documentation
  (List $.Documentation)
  (list ($.module /._
                  "")

        ($.definition /.equivalence)
        ($.definition /.number)
        ($.definition /.invalid_month)
        ($.definition /.by_number)
        ($.definition /.hash)
        ($.definition /.order)
        ($.definition /.enum)
        ($.definition /.not_a_month_of_the_year)
        ($.definition /.codec)

        ($.definition /.Month
          "A month of the year.")

        ($.definition /.days
          "The amount of days of a month."
          ($.example (' (days month))))

        ($.definition /.leap_year_days
          "The amount of days of a month (in a leap year)."
          ($.example (' (leap_year_days month))))

        ($.definition /.year
          "All the months, ordered by when they come in a year.")
        ))
