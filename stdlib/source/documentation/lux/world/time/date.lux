(.require
 [library
  [lux (.except and)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.definition /.invalid_day)
             ($.definition /.epoch)
             ($.definition /.year)
             ($.definition /.month)
             ($.definition /.day_of_month)
             ($.definition /.equivalence)
             ($.definition /.order)
             ($.definition /.invalid_month)
             ($.definition /.parser)
             ($.definition /.days)
             ($.definition /.of_days)
             ($.definition /.enum)

             ($.definition /.Date
               "A date specified as a year/month/day triplet.")

             ($.definition /.date
               "A date, within the allowed limits."
               [(date year month day_of_month)])

             ($.definition /.codec
               (format "Based on ISO 8601."
                       \n "For example: 2017-01-15"))]
            []))
