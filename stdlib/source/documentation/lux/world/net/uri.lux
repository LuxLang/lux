(.require
 [library
  [lux (.except)
   [program (.only program:)]
   ["$" documentation]
   ["[0]" debug]
   [control
    ["[0]" io]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.documentation /.URI
               "A Uniform Resource Identifier.")

             ($.documentation /.separator
               "A separator for the pieces of a URI.")]
            []))
