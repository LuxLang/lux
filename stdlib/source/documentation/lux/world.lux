(.module:
  [library
   [lux #*
    [program (#+ program:)]
    ["$" documentation (#+ documentation:)]
    ["." debug]
    [control
     ["." io]]
    [data
     [collection
      ["." list ("#\." monoid)]]]]]
  ["." / #_
   ["#." console]
   ["#." file]
   ["#." input #_
    ["#/." keyboard]]
   ["#." net]
   ["#." output #_
    ["#/." video #_
     ["#/." resolution]]]
   ["#." program]
   ["#." shell]])

(.def: .public documentation
  (.List $.Module)
  ($_ list\composite
      /console.documentation
      /file.documentation
      /input/keyboard.documentation
      /net.documentation
      /output/video/resolution.documentation
      /program.documentation
      /shell.documentation))
