(.using
 [library
  [lux (.except char)
   ["$" documentation (.only documentation:)]
   [data
    [text
     ["%" format (.only format)]]
    [collection
     ["[0]" list (.open: "[1]#[0]" monoid)]]]]]
 ["[0]" /
  ["[1][0]" continuous]
  ["[1][0]" fuzzy]])

(.def: .public documentation
  (.List $.Module)
  (all list#composite
       /continuous.documentation
       /fuzzy.documentation))
