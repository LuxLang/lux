(.module:
  [library
   [lux (#- char)
    ["$" documentation (#+ documentation:)]
    [data
     [text
      ["%" format (#+ format)]]
     [collection
      ["." list ("#\." monoid)]]]]]
  ["." / #_
   ["#." continuous]
   ["#." fuzzy]])

(.def: .public documentation
  (.List $.Module)
  ($_ list\compose
      /continuous.documentation
      /fuzzy.documentation))
