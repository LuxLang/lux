(.module:
  [library
   [lux {"-" [char]}
    ["$" documentation {"+" [documentation:]}]
    [data
     [text
      ["%" format {"+" [format]}]]
     [collection
      ["[0]" list ("[1]\[0]" monoid)]]]]]
  ["[0]" / "_"
   ["[1][0]" continuous]
   ["[1][0]" fuzzy]])

(.def: .public documentation
  (.List $.Module)
  ($_ list\composite
      /continuous.documentation
      /fuzzy.documentation))
