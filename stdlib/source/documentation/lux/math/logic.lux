(.require
 [library
  [lux (.except)
   ["$" documentation]
   [data
    [text
     ["%" \\format (.only format)]]
    [collection
     ["[0]" list (.use "[1]#[0]" monoid)]]]]]
 ["[0]" /
  ["[1][0]" continuous]
  ["[1][0]" fuzzy]])

(def .public documentation
  (List $.Documentation)
  (all list#composite
       /continuous.documentation
       /fuzzy.documentation
       ))
