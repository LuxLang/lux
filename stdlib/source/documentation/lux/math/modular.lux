(.require
 [library
  [lux (.except private)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.default /.modulus)
             ($.default /.value)
             ($.default /.incorrect_modulus)
             ($.default /.codec)

             ($.default /.=)
             ($.default /.<)
             ($.default /.<=)
             ($.default /.>)
             ($.default /.>=)

             ($.default /.equivalence)
             ($.default /.order)

             ($.default /.+)
             ($.default /.-)
             ($.default /.*)
             
             ($.default /.addition)
             ($.default /.multiplication)
             ($.default /.inverse)
             ($.default /.moduli_are_not_equal)

             ($.definition (/.Mod %)
               "A number under a modulus.")

             ($.definition /.modular
               ""
               [(modular modulus value)])

             ($.definition /.adapter
               ""
               [(adapter reference subject)])]
            []))
