(.require
 [library
  [lux (.except private)
   ["$" documentation]
   [data
    ["[0]" text (.only \n)
     ["%" \\format (.only format)]]]]]
 [\\library
  ["[0]" /]])

(.def .public documentation
  (.List $.Module)
  ($.module /._
            ""
            [($.definition /.zero_cannot_be_a_modulus)
             ($.definition /.modulus)
             ($.definition /.divisor)
             ($.definition /.=)

             ($.definition (/.Modulus %)
               (format "A number used as a modulus in modular arithmetic."
                       \n "It cannot be 0."))

             ($.definition /.congruent?
               ""
               [(congruent? modulus reference subject)])

             ($.definition /.literal
               ""
               ["Success!"
                (literal 123)]
               ["Failure!"
                (literal 0)])]
            []))
