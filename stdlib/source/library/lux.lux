              [_ {#Form {#Item type_fn args}}]
              (<| (function' [lux])
                  (meta#let lux [type_fn (normal_type type' type_fn)])
                  (meta#let lux [args (monad#each#meta (normal_type type') args)])
                  (meta#return lux (type_reification type_fn args)))

              _
              (meta#in it)))

