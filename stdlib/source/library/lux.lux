         ({(#Item type #End)
           (do meta_monad
             [type+ (full_expansion type)]
             ({(#Item type' #End)
               (in (list (normal_type type')))
               
               _
               (failure "The expansion of the type-syntax had to yield a single element.")}
              type+))

           _
           (failure "Wrong syntax for type")}
          tokens))

