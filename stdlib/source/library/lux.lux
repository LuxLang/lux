  (do meta_monad
    [=module (..module module)
     .let [[..#module_hash _
            ..#module_aliases _
            ..#definitions definitions
            ..#imports _
            ..#module_state _] =module]]
    (case (plist#value name definitions)
      {#Some {#Slot [exported type group index]}}
      (in_meta [index
                (list#each (function (_ slot)
                             [module slot])
                           group)
                exported
                type])

      _
