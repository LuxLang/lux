        (-> (-> input
                ($ Meta output))
            (-> ($ List input)
                ($ Meta ($ List output)))))
      (function' [lux]
                 ((.is# (for_any (_ input output)
                          (-> Lux (-> input ($ Meta output)) ($ List input) ($ List output)
                              ($ Either Text (And Lux ($ List output)))))
                        (function' again [lux $ items output]
                                   (.when# items
                                           {#End}
                                           {#Right [lux (list#reversed output)]}
                                           
                                           {#Item head tail}
                                           (.when# ($ head lux)
                                                   {#Right [lux head]}
