      [=raw (..embedded_expansions raw)
       .let [[labels labelled] =raw]]
      (in (list (` (with_expansions [(~+ (|> labels
                                             (list#each (function (_ [label expansion]) (list label expansion)))
                                             list#conjoint))]
                     (~ labelled))))))

    _
    (failure (..wrong_syntax_error (symbol ..``)))))

