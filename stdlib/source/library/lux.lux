                       (^ (list (~+ (list#each local_symbol$ args))))
                       {.#Right [(~ g!compiler)
                                 (list (~+ (list#each (function (_ template)
                                                        (` (`' (~ (with_replacements rep_env
                                                                    template)))))
                                                      input_templates)))]}
                       
                       (~ g!_)
                       {.#Left (~ (text$ (..wrong_syntax_error [this_module name])))}))))))

