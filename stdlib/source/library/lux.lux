(def' .private (normal_type type' it)
      (-> (-> Code ($ Meta Code)) Code
          ($ Meta Code))
      (.when# it
              [_ {#Variant {#Item [_ {#Name ["library/lux" "#Named"]}] {#Item name {#Item anonymous {#End}}}}}]
              (<| (function' [lux])
                  (meta#let lux [anonymous (normal_type type' anonymous)])
                  (meta#return lux (` {..#Named (, name) (, anonymous)})))

              [_ {#Variant {#Item [_ {#Name name}] parts}}]
              (<| (function' [lux])
                  (meta#let lux [parts (monad#each#meta (normal_type type') parts)])
                  (meta#return lux (` {(, (as_name name)) (,* parts)})))

              [_ {#Tuple members}]
              (<| (function' [lux])
                  (meta#let lux [members (monad#each#meta (normal_type type') members)])
                  (meta#return lux (` (Tuple (,* members)))))

              [_ {#Form {#Item [_ {#Name ["library/lux" "in_module#"]}]
                               {#Item [_ {#Text module}]
                                      {#Item it'
                                             {#End}}}}}]
              (<| (function' [lux])
                  (meta#let lux [it' (normal_type type' it')])
                  (meta#return lux (` (.in_module# (, (as_text module)) (, it')))))

              [_ {#Form {#Item [_ {#Name ["" ","]}] {#Item expression {#End}}}}]
              (meta#in expression)

              [_0 {#Form {#Item [_ {#Name ["library/lux" "when#"]}]
                                {#Item value
                                       {#Item binding
                                              {#Item body
                                                     {#End}}}}}}]
              (<| (function' [lux])
                  (meta#return lux [_0 {#Form (list [_ {#Name ["library/lux" "when#"]}]
                                                    value
                                                    binding
                                                    (` (..type (, body))))}]))

              [_0 {#Form {#Item [_1 {#Name ["library/lux" "__adjusted_quantified_type__"]}]
                                {#Item _permission
                                       {#Item _level
                                              {#Item body
                                                     {#End}}}}}}]
              (<| (function' [lux])
                  (meta#let lux [body (normal_type type' body)])
                  (meta#return lux [_0 {#Form (list [_1 {#Name [..prelude "__adjusted_quantified_type__"]}]
                                                    _permission
                                                    _level
                                                    body)}]))

              [_ {#Form {#Item [_ {#Form {#Item [_ {#Name ["library/lux" "in_module#"]}]
                                                {#Item [_ {#Text "library/lux"}]
                                                       {#Item [_ {#Name ["library/lux" "type"]}]
                                                              {#End}}}}}]
                               {#Item it'
                                      {#End}}}}]
              (type' it')

              [_0 {#Form {#Item [_1 {#Name binding}]
                                parameters}}]
              (<| (function' [lux])
                  (.when# binding
                          ["" local]
                          (.when# (in_env local lux)
                                  {.#Some type_of_local}
                                  ((normal_type_reification (normal_type type') [_1 {#Name binding}] parameters) lux)
                                  
                                  not_a_local
                                  ((expanded_type_reification (normal_type type') _1 binding parameters) lux))
                          
