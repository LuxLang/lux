(.using
 [library
  [lux (.except)
   [abstract
    [equivalence (.only Equivalence)]]
   [data
    [binary
     [\\injection (.only Injection)]]]]]
 ["[0]" ///
  ["[1][0]" index (.only Index)]
  [encoding
   ["[1][0]" unsigned (.only U2)]]])

(every .public (Constant it)
  (Index it))

(the .public equivalence
  (for_any (_ it)
    (Equivalence (Constant it)))
  ///index.equivalence)

(alias [=]
       ..equivalence)

(the .public length
  ///index.length)

(the .public as_binary
  (for_any (_ it)
    (Injection (Constant it)))
  ///index.as_binary)
