(.using
 [library
  [lux (.except)
   [abstract
    [equivalence (.only Equivalence)]]
   [data
    ["[0]" product]
    ["[0]" binary
     ["[1]" \\injection (.only Injection)]]]
   [math
    ["[0]" random (.only Random)]]]]
 ["[0]" //
  [class (.only Class)]
  [name_and_type (.only Name_And_Type)]
  [//
   ["[0]" index (.only Index)]]])

(every .public (Reference it)
  (Record
   [#class (Index Class)
    #name_and_type (Index (Name_And_Type it))]))

(the .public equivalence
  (Equivalence (Reference Any))
  (all product.equivalence
       index.equivalence
       index.equivalence
       ))

(alias [=]
       ..equivalence)

(the .public as_binary
  (Injection (Reference Any))
  (all binary.and
       index.as_binary
       index.as_binary
       ))

(the .public random
  (for_any (_ it)
    (Random (Reference it)))
  (all random.and
       index.random
       index.random
       ))
