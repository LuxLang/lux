(.using
 [library
  [lux (.except)
   [abstract
    [equivalence (.only Equivalence)]]
   [data
    [binary
     [\\injection (.only Injection)]]]
   [math
    [number
     ["[0]" /32
      ["[1]" integer]]]]]]
 [///
  [encoding
   ["[0]" signed]]])

(every .public Jump
  signed.S2)

(the .public equivalence
  (Equivalence Jump)
  signed.equivalence)

(alias [=]
       ..equivalence)

(the .public length
  signed.bytes/2)

(the .public as_binary
  (Injection Jump)
  signed.as_binary/2)

(every .public Big
  /32.Number)

(the .public big
  (-> Jump
      Big)
  (|>> signed.value
       /32.of))
