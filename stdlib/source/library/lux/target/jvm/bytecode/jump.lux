(.module:
  [library
   [lux #*
    [abstract
     [equivalence (#+ Equivalence)]]
    [data
     ["." format #_
      ["#" binary (#+ Writer)]]]]]
  ["." /// #_
   [encoding
    ["#." signed (#+ S2 S4)]]])

(type: .public Jump S2)

(def: .public equivalence
  (Equivalence Jump)
  ///signed.equivalence)

(def: .public writer
  (Writer Jump)
  ///signed.writer/2)

(type: .public Big_Jump S4)

(def: .public lifted
  (-> Jump Big_Jump)
  ///signed.lifted/4)
