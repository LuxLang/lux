(.using
 [library
  [lux "*"
   [control
    [try {"+" Try}]]
   [data
    [binary {"+" Binary}]
    ["[0]" product]
    [collection
     [dictionary {"+" Dictionary}]
     ["[0]" sequence]
     ["[0]" list ("[1]#[0]" functor)]]]
   [world
    ["[0]" file]]]]
 [//
  ["[0]" cache "_"
   [dependency
    ["[1]/[0]" module]]]
  ["[0]" archive {"+" Archive}
   ["[0]" artifact]
   ["[0]" registry]
   ["[0]" unit]
   ["[0]" module
    ["[0]" descriptor]]]])

(type: .public Packager
  (-> (Dictionary file.Path Binary)
      Archive
      (Maybe unit.ID)
      (Try (Either Binary
                   (List [Text Binary])))))

(type: .public Order
  (List [module.ID (List artifact.ID)]))

(def: .public order
  (-> (cache/module.Order Any) Order)
  (list#each (function (_ [module [module_id entry]])
               (|> entry
                   (the archive.#registry)
                   registry.artifacts
                   sequence.list
                   (list#each (|>> product.left (the artifact.#id)))
                   [module_id]))))
