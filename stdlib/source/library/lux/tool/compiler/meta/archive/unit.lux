(.using
 [library
  [lux "*"
   [abstract
    [equivalence {"+" Equivalence}]
    [hash {"+" Hash}]]
   [data
    ["[0]" product]
    [collection
     ["[0]" set {"+" Set}]]]
   [math
    [number
     ["[0]" nat]]]]]
 [//
  ["[0]" module]
  ["[0]" artifact]])

(type: .public ID
  (Record
   [#module module.ID
    #artifact artifact.ID]))

(def: .public hash
  (Hash ID)
  ($_ product.hash
      nat.hash
      nat.hash))

(def: .public equivalence
  (Equivalence ID)
  (# ..hash &equivalence))

(def: .public none
  (Set ID)
  (set.empty ..hash))
