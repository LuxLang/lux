(.module:
  [library
   [lux (#- type)
    [control
     ["." try]]
    [target
     [jvm
      ["_" bytecode (#+ Bytecode)]
      [encoding
       [name (#+ External)]
       ["." signed]]
      ["." type]]]]]
  ["." ///// #_
   ["#." abstract]])

(def: .public field "partials")
(def: .public type type.int)

(def: .public initial
  (Bytecode Any)
  (|> +0 signed.s1 try.assumed _.bipush))

(def: this
  _.aload_0)

(def: .public value
  (Bytecode Any)
  ($_ _.compose
      ..this
      (_.getfield /////abstract.class ..field ..type)
      ))
