(.require
 [library
  [lux (.except i64)
   [meta
    [target
     ["_" scheme (.only Expression)]]]]])

(with_template [<name> <type> <code>]
  [(def .public <name>
     (-> <type> Expression)
     <code>)]

  [bit  Bit       _.bool]
  [i64  (I64 Any) (|>> .int _.int)]
  [f64  Frac      _.float]
  [text Text      _.string]
  )
