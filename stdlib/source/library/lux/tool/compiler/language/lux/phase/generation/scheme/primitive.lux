(.module:
  [library
   [lux (#- i64)
    [target
     ["_" scheme (#+ Expression)]]]])

(template [<name> <type> <code>]
  [(def: #export <name>
     (-> <type> Expression)
     <code>)]

  [bit  Bit       _.bool]
  [i64  (I64 Any) (|>> .int _.int)]
  [f64  Frac      _.float]
  [text Text      _.string]
  )
