(.module:
  [library
   [lux #*
    ["." ffi]
    [abstract
     ["." monad (#+ do)]]
    [control
     ["<>" parser
      ["<c>" code (#+ Parser)]]]
    [data
     [collection
      ["." array (#+ Array)]
      ["." dictionary]
      ["." list]]]
    ["." type
     ["." check]]
    ["@" target
     ["_" common_lisp]]]]
  [//
   ["/" lux (#+ custom)]
   [//
    ["." bundle]
    [//
     ["." analysis #_
      ["#/." type]]
     [//
      ["." analysis (#+ Analysis Operation Phase Handler Bundle)]
      [///
       ["." phase]]]]]])

(def: .public bundle
  Bundle
  (<| (bundle.prefix "common_lisp")
      (|> bundle.empty
          )))
