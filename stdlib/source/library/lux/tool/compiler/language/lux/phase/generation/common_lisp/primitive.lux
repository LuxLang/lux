(.module:
  [library
   [lux {"-" [i64]}
    [target
     ["_" common_lisp {"+" [Expression]}]]]])

(def: .public bit
  (-> Bit (Expression Any))
  _.bool)

(def: .public i64
  (-> (I64 Any) (Expression Any))
  (|>> .int _.int))

(def: .public f64
  (-> Frac (Expression Any))
  _.double)

(def: .public text
  (-> Text (Expression Any))
  _.string)
