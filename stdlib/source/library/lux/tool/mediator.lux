(.using
 [library
  [lux {"-" Source Module}
   [world
    ["[0]" binary (.only Binary)]
    ["[0]" file (.only Path)]]]]
 [//
  [compiler (.only Compiler)
   [meta
    ["[0]" archive (.only Archive)
     [descriptor (.only Module)]]]]])

(type: .public Source
  Path)

(type: .public (Mediator !)
  (-> Archive Module (! Archive)))

(type: .public (Instancer ! d o)
  (-> (file.System !) (List Source) (Compiler d o) (Mediator !)))
