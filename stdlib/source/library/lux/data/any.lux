(.require
 [library
  [lux (.except)
   [abstract
    [equivalence (.only Equivalence)]
    [hash (.only Hash)]]]])

(def .public any
  Any
  [])

(def .public equivalence
  (Equivalence Any)
  (implementation
   (def (= _ _)
     true)))

(def .public hash
  (Hash Any)
  (implementation
   (def equivalence
     ..equivalence)
   
   (def (hash _)
     1)))
