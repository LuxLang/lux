(.module:
  [library
   [lux #*
    [type
     abstract]
    [control
     [parser
      ["s" code]]]
    ["." macro
     [syntax (#+ syntax:)]]
    [world
     [net (#+ URL)]]]]
  ["." // #_
   ["#." value (#+ Value Font_Stretch Font_Style Font_Weight)]])

(type: #export Unicode_Range
  {#start Nat
   #end Nat})

(type: #export Font
  {#family Text
   #source URL
   #stretch (Maybe (Value Font_Stretch))
   #style (Maybe (Value Font_Style))
   #weight (Maybe (Value Font_Weight))
   #unicode_range (Maybe Unicode_Range)})
