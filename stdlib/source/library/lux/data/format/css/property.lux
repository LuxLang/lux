(syntax: (text_identifier {identifier s.text})
  (in (list (code.local_identifier (text.replace_all "-" "_" identifier)))))

(abstract: .public (Property brand)
  {}
  
  Text

  (def: .public name
    (-> (Property Any) Text)
    (|>> :representation))

  (template [<brand> <alias>+ <property>+]
    [(`` (template [<alias> <property>]
           [(def: .public <alias>
              (Property <brand>)
              (:abstraction <property>))]

           (~~ (template.spliced <alias>+))))

     (with_expansions [<rows> (template.spliced <property>+)]
       (template [<property>]
         [(`` (def: .public (~~ (text_identifier <property>))
                (Property <brand>)
                (:abstraction <property>)))]
         
         <rows>))]

