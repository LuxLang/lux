(.require
 [library
  [lux (.except)
   ["[0]" ffi]
   [data
    ["[0]" text]]
   [meta
    [macro
     ["[0]" template]]
    [compiler
     ["@" target]]]]])

(every .public Event
  Text)

... https://developer.mozilla.org/en-US/docs/Web/Events
(with_template [<name>]
  [(the .public <name>
     Event
     (|> (template.text [<name>])
         (text.replaced "dom_content_loaded" "DOMContentLoaded")
         (text.replaced "double" "dbl")
         (text.replaced "_" "")))]

