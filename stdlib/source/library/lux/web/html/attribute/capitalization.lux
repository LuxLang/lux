(.using
 [library
  [lux (.except)
   [meta
    [macro
     ["[0]" template]]]]])

(every .public Capitalization
  Text)

(template.with [<name>]
  [(the .public <name>
     Capitalization
     (template.text [<name>]))]

  [none]
  [sentences]
  [words]
  [characters]
  )
