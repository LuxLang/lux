(.using
 [library
  [lux (.except)
   [data
    ["[0]" text]]
   [meta
    [macro
     ["[0]" template]]]]])

... https://developer.mozilla.org/en-US/docs/Web/HTML/Element/img#decoding
(every .public Decoding
  Text)

(template.with [<name>]
  [(the .public <name>
     Decoding
     (|> (template.text [<name>])
         (text.replaced "hronous" "")
         (text.replaced "matic" "")))]

  [synchronous]
  [asynchronous]
  [automatic]
  )
