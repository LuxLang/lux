(.require
 [library
  [lux (.except)
   [data
    ["[0]" text]]
   [meta
    [macro
     ["[0]" template]]]]])

(every .public Pragma
  Text)

(with_template [<name>]
  [(the .public <name>
     Pragma
     (|> (template.text [<name>])
         (text.replaced "_" "-")))]

  [content_security_policy]
  [content_type]
  [default_style]
  [x_ua_compatible]
  [refresh]
  )
