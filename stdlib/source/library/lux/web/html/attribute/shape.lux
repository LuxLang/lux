(.using
 [library
  [lux (.except)
   [data
    ["[0]" text]]
   [meta
    [macro
     ["[0]" template]]]]])

... https://developer.mozilla.org/en-US/docs/Web/HTML/Element/area#attributes
(every .public Shape
  Text)

(template.with [<name>]
  [(the .public <name>
     Shape
     (|> (template.text [<name>])
         (text.replaced "rectangle" "rect")
         (text.replaced "polygon" "poly")))]

  [rectangle]
  [circle]
  [polygon]
  [default]
  )
