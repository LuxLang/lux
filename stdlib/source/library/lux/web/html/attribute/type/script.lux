(.using
 [library
  [lux (.except Type)
   [data
    ["[0]" text]]
   [macro
    ["[0]" template]]
   [world
    [net
     ["[0]" mime (.only MIME)]]]]])

... https://developer.mozilla.org/en-US/docs/Web/HTML/Element/script#attributes
(every .public Type
  Text)

(template.with [<name>]
  [(the .public <name>
     Type
     (|> (template.text [<name>])
         (text.replaced "_" "")))]

  [module]
  [import_map]
  )

(the .public javascript
  Type
  (mime.name mime.javascript))

(the .public data
  (-> MIME
      Type)
  mime.name)
