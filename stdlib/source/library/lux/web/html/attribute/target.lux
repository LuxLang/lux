(.using
 [library
  [lux (.except Target)
   [data
    ["[0]" text]]
   [macro
    ["[0]" template]]]])

(every .public Target
  Text)

(template.with [<name>]
  [(the .public <name>
     Target
     (template.text ["_" <name>]))]

  [self]
  [blank]
  [parent]
  [top]
  )
