(.require
 [library
  [lux (.except)
   [data
    ["[0]" text]]
   [meta
    [macro
     ["[0]" template]]]]])

(every .public Target
  Text)

(with_template [<name>]
  [(the .public <name>
     Target
     (template.text ["_" <name>]))]

  [self]
  [blank]
  [parent]
  [top]
  )
