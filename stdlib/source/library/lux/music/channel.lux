(.require
 [library
  [lux (.except)
   [control
    [function
     [predicate (.only Predicate)]]]
   [math
    [number (.only hex)
     ["n" nat]]]
   [meta
    [macro
     ["[0]" template]]]]])

(every .public Channel
  Nat)

(template.with [<it> <name>]
  [(the .public <name>
     Channel
     (hex <it>))]

  ["0" minimum]
  ["F" maximum]

  ["A" percussion]
  )

(the .public tonal?
  (Predicate Channel)
  (|>> (n.= ..percussion)
       not))
