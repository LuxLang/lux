(.require
 [library
  [lux (.except)
   [control
    [function
     [predicate (.only Predicate)]]]
   [math
    [number (.only hex)
     ["n" nat]]]]])

(type .public Channel
  Nat)

(with_template [<it> <name>]
  [(def .public <name>
     Channel
     (hex <it>))]

  ["0" minimum]
  ["F" maximum]

  ["A" percussion]
  )

(def .public tonal?
  (Predicate Channel)
  (|>> (n.= ..percussion)
       not))
