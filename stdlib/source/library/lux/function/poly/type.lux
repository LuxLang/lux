(every .public (Constant it)
  (for_any (_ _)
    it))

(every .public Variable
  (for_any (_ it)
    it))

(every .public (Recursive context)
  (for_any (self it)
    (context self it)))

(every .public (Sum left right)
  (for_any (_ it)
    (Or (left it)
        (right it))))

(every .public (Product left right)
  (for_any (_ it)
    (And (left it)
         (right it))))
