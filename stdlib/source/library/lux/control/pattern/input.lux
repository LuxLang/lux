(.require
 [library
  [lux (.except)]])

(every .public Zero
  [])

(the .public zero
  (template (_)
    [[]]))

(every .public (One of)
  of)

(every .public (Succ of pred)
  (And of pred))

(the .public succ
  (template (_ of pred)
    [[of pred]]))
