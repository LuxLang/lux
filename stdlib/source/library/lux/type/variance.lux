(.module:
  [library
   [lux #*]])

(type: .public (Co t)
  {#.doc (example "A constraint for covariant types.")}
  (-> Any t))

(type: .public (Contra t)
  {#.doc (example "A constraint for contravariant types.")}
  (-> t Any))

(type: .public (In t)
  {#.doc (example "A constraint for invariant types.")}
  (-> t t))
