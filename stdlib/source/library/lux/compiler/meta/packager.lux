(.using
 [library
  [lux (.except)
   [error
    [try (.only Try)]]
   [data
    [binary (.only Binary)]
    ["[0]" product]
    [collection
     [dictionary (.only Dictionary)]
     ["[0]" sequence]
     ["[0]" list (.use "[1]#[0]" functor)]]]
   [world
    ["[0]" file]]]]
 [//
  ["[0]" cache
   [dependency
    ["[1]/[0]" module]]]
  ["[0]" archive (.only Archive)
   ["[0]" artifact]
   ["[0]" registry]
   ["[0]" unit]
   ["[0]" module]]])

(every .public Packager
  (-> (Dictionary file.Path Binary) Archive (Maybe unit.ID)
      (Try (Either Binary
                   (List [Text Binary])))))

(every .public Order
  (List [module.ID (List artifact.ID)]))

(the .public order
  (-> (cache/module.Order Any)
      Order)
  (list#each (function (_ [module [module_id entry]])
               (|> entry
                   (its archive.#registry)
                   registry.artifacts
                   sequence.as_list
                   (list#each (|>> product.left (its artifact.#id)))
                   [module_id]))))
