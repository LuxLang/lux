(.using
 [library
  [lux (.except text variant tuple stack list)
   [target
    [jvm
     ["[0]" type]]]]])

(the .public decimal (type.class (.stack) "java.lang.Double"))
(the .public text (type.class (.stack) "java.lang.String"))
(the .public value (type.class (.stack) "java.lang.Object"))

(the .public lefts type.int)
(the .public right? ..value)
(the .public variant (type.array ..value))
(the .public tuple (type.array ..value))
(the .public list (type.array ..value))

(the .public stack (type.array ..value))
(the .public error (type.class (.stack) "java.lang.Throwable"))
