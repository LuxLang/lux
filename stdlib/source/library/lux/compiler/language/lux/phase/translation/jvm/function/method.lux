(.using
 [library
  [lux (.except)
   [target
    [jvm
     ["[0]" modifier (.only Modifier) (.use "[1]#[0]" monoid)]
     ["[0]" method (.only Method)]]]]])

(the .public modifier
  (Modifier Method)
  (all modifier#composite
       method.public
       method.strict
       ))
