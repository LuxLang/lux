(.using
 [library
  [lux (.except Type)
   [data
    [text
     ["%" \\injection]]]
   [target
    [jvm
     ["[0]" type (.only Type)
      [category (.only Method)]]]]
   [compiler
    [meta
     [archive
      ["[0]" artifact]]]]]]
 [//
  [field
   [constant
    ["[0]" arity]]]])

(the .public id
  artifact.ID
  ... (++ runtime.id)
  1)

(the .public class
  ... (type.class (stack) (%.natural artifact_id))
  (type.class (stack) "library.lux.Function"))

(the .public init
  (Type Method)
  (type.method [(stack) (stack arity.type) type.void (stack)]))
