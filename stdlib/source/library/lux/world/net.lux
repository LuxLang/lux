(.module:
  [library
   [lux (#- Location)]])

(type: .public Address
  {#.doc (example "A TCP/IP address.")}
  Text)

(type: .public Port
  {#.doc (example "A TCP/IP port.")}
  Nat)

(type: .public URL
  {#.doc (example "A Uniform Resource Locator.")}
  Text)

(type: .public Location
  {#address Address
   #port Port})
