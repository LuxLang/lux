(.require
 [library
  [lux (.except)
   [meta
    [macro
     ["[0]" template]
     ["[0]" expansion]]]]])

(every .public Port
  Nat)

(template.with [<port> <name> <aliases>']
  [(the .public <name>
     Port
     <port>)

   (expansion.let [<aliases> (template.spliced <aliases>')]
     (template.with [<alias>]
       [(the .public <alias> <name>)]

       <aliases>
       ))]

