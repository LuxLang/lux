(.require
 [library
  [lux (.except)
   [abstract
    [equivalence (.only Equivalence)]
    [hash (.only Hash)]]
   [data
    ["[0]" product]]
   [math
    [number
     ["[0]" nat]]]
   [meta
    [macro
     ["[0]" template]]]]])

(every .public Resolution
  (Record
   [#width Nat
    #height Nat]))

(the .public hash
  (Hash Resolution)
  (product.hash nat.hash nat.hash))

(the .public equivalence
  (Equivalence Resolution)
  (of ..hash equivalence))

(alias [=]
       ..equivalence)

... https://en.wikipedia.org/wiki/Display_resolution#Common_display_resolutions
(template.with [<name> <width> <height>]
  [(the .public <name>
     Resolution
     [#width <width>
      #height <height>])]

  [svga        800  600]
  [wsvga      1024  600]
  [xga        1024  768]
  [xga+       1152  864]
  [wxga_16:9  1280  720]
  [wxga_5:3   1280  768]
  [wxga_16:10 1280  800]
  [sxga       1280 1024]
  [wxga+      1440  900]
  [hd+        1600  900]
  [wsxga+     1680 1050]
  [fhd        1920 1080]
  [wuxga      1920 1200]
  [wqhd       2560 1440]
  [uhd_4k     3840 2160]
  )
