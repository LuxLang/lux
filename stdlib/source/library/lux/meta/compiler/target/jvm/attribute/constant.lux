(.require
 [library
  [lux (.except)
   [abstract
    [equivalence (.only Equivalence)]]
   [data
    [binary
     [\\injection (.only Injection)]]]]]
 ["[0]" ///
  [constant (.only Value)]
  ["[1][0]" index (.only Index)]
  [encoding
   ["[1][0]" unsigned (.only U2 U4)]]])

(every .public (Constant a)
  (Index (Value a)))

(the .public equivalence
  (All (_ a) (Equivalence (Constant a)))
  ///index.equivalence)

(the .public length
  ///index.length)

(the .public format
  (All (_ a) (Injection (Constant a)))
  ///index.format)
