(.using
 [library
  [lux (.except)
   [math
    [number
     ["n" natural]]]
   [macro
    ["[0]" template]]]])

(every .public Arity
  Natural)

(template.with [<comparison> <name>]
  [(the .public <name> (-> Arity Bit) (<comparison> 1))]

  [n.< nullary?]
  [n.= unary?]
  [n.> multiary?]
  )
