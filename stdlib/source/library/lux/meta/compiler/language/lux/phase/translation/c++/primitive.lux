(.require
 [library
  [lux (.except i64)
   [meta
    [compiler
     [target
      ["_" c++ (.only Literal Expression)]]]]]])

(def .public bit
  (-> Bit
      Literal)
  _.bool)

(def .public i64
  (-> (I64 Any)
      Expression)
  (|>> .int
       _.int
       _.int64_t))

(def .public f64
  (-> Frac
      Literal)
  _.double)

(def .public text
  (-> Text
      Literal)
  _.u32string)
