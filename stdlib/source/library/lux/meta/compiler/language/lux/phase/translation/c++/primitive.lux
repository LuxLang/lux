(.require
 [library
  [lux (.except i64)
   [meta
    [compiler
     [target
      ["_" c++]]]]]]
 [//
  ["[0]" runtime]
  ["[0]" type]])

(def .public bit
  (-> Bit
      _.Expression)
  (|>> _.bool
       (runtime.simple type.bit)))

(def .public i64
  (-> (I64 Any)
      _.Expression)
  (|>> .int
       _.int
       _.int64_t
       (runtime.simple type.i64)))

(def .public f64
  (-> Frac
      _.Expression)
  (|>> _.double
       (runtime.simple type.f64)))

(def .public text
  (-> Text
      _.Expression)
  (|>> _.u32_string
       (runtime.simple type.text)))
