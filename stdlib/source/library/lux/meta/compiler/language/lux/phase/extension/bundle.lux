(.require
 [library
  [lux (.except)
   [data
    ["[0]" text]
    [collection
     ["[0]" dictionary]]]]]
 [// (.only Handler Bundle)])

(def .public empty
  Bundle
  (dictionary.empty text.hash))

(def .public (install name anonymous)
  (All (_ s i o)
    (-> Text (-> Text (Handler s i o))
        (-> (Bundle s i o) (Bundle s i o))))
  (dictionary.has name (anonymous name)))
