(.require
 [library
  [lux (.except i64)
   [meta
    [compiler
     [target
      ["_" ruby (.only Literal)]]]]]])

(with_template [<type> <name> <implementation>]
  [(def .public <name>
     (-> <type> Literal)
     <implementation>)]

  [Bit       bit  _.bool]
  [(I64 Any) i64  (|>> .int _.int)]
  [Frac      f64  _.float]
  [Text      text _.string]
  )
