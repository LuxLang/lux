(.module:
  {#.doc (.example "Syntax for marking a definition as an export.")}
  [library
   [lux #*
    [control
     ["<>" parser ("#\." monad)
      ["<.>" code (#+ Parser)]]]]])

(def: .public default_policy
  Code
  (` .private))

(def: .public (parser un_exported)
  (All [a] (-> (Parser a) (Parser [Code a])))
  (<>.either (<>.and <code>.any un_exported)
             (<>.and (<>\in ..default_policy) un_exported)))
