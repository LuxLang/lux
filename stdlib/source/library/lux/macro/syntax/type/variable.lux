(.require
 [library
  [lux (.except)
   [abstract
    [equivalence (.only Equivalence)]]
   [data
    ["[0]" text]]
   [macro
    ["[0]" code (.only)
     ["<[1]>" \\parser (.only Parser)]]]]])

(type .public Variable
  Text)

(def .public equivalence
  (Equivalence Variable)
  text.equivalence)

(def .public format
  (-> Variable Code)
  code.local)

(def .public parser
  (Parser Variable)
  <code>.local)
