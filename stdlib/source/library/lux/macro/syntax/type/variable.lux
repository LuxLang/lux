(.module:
  [library
   [lux "*"
    [abstract
     [equivalence {"+" [Equivalence]}]]
    [control
     [parser
      ["<.>" code {"+" [Parser]}]]]
    [data
     ["." text]]
    [macro
     ["." code]]]])

(type: .public Variable
  Text)

(def: .public equivalence
  (Equivalence Variable)
  text.equivalence)

(def: .public format
  (-> Variable Code)
  code.local_identifier)

(def: .public parser
  (Parser Variable)
  <code>.local_identifier)
