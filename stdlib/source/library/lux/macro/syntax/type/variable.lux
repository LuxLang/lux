(.module:
  [library
   [lux "*"
    [abstract
     [equivalence {"+" Equivalence}]]
    [control
     [parser
      ["<[0]>" code {"+" Parser}]]]
    [data
     ["[0]" text]]
    [macro
     ["[0]" code]]]])

(type: .public Variable
  Text)

(def: .public equivalence
  (Equivalence Variable)
  text.equivalence)

(def: .public format
  (-> Variable Code)
  code.local_identifier)

(def: .public parser
  (Parser Variable)
  <code>.local_identifier)
