(.using
 [library
  [lux (.except)
   [abstract
    [equivalence (.only Equivalence)]]
   [control
    [parser
     ["<[0]>" code (.only Parser)]]]
   [data
    ["[0]" text]]
   [macro
    ["[0]" code]]]])

(type: .public Variable
  Text)

(def .public equivalence
  (Equivalence Variable)
  text.equivalence)

(def .public format
  (-> Variable Code)
  code.local)

(def .public parser
  (Parser Variable)
  <code>.local)
