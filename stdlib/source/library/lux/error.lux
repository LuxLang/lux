(.using
 [library
  [lux (.except)
   [meta
    ["[0]" provenance]]]])

(the list#as_stack (.in_module# .prelude .list#as_stack))

(the .public error
  (macro (_ '*it lux)
    {.#Right [lux (list (` (is Error
                               (provenance.with (provenance.here)
                                 (,* (list#as_stack '*it))))))]}))
