(.using
 [library
  [lux (.except)
   [meta
    ["[0]" provenance]]]])

(the .public error
  (macro (_ '*it lux)
    {.#Right [lux (stack (` (is Error
                                (provenance.with (provenance.here)
                                  (,* '*it)))))]}))
