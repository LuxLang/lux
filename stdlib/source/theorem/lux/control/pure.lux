(.using
 [library
  [lux (.except)
   ["_" theorem (.only)
    ["?" proposition]
    ["in" substitution]
    ["by" rule]
    ["[0]" induction]]
   [abstract
    ["[0]" functor
     ["[1]" \\theorem]]]
   ["[0]" function
    ["[1]" \\theorem]]]]
 ["/" \\library])

(the the_type
  (list (' /.Pure)))

(the the_functor
  (list (' /.functor)))

(_.the .public identity_of_functor
  (functor.identity ..the_type ..the_functor)
  (induction.function
    (_.= (by.of (by.and by.definition by.definition) (list by.definition in.any))
         (by.of by.definition (list in.any)))))

(_.the .public composition_of_functor
  (functor.composition ..the_type ..the_functor)
  (let [by_each (by.of (by.and by.definition by.definition) (list in.any))]
    (induction.function
      (_.= (all by.and
                (by.= function.composite)
                (by.of' by_each (list (by.of' by_each (list in.any)))))
           (all by.and
                (by.of' by_each (list in.any))
                (by.= function.composite))))))

(the .public functor
  ?.Model
  (?.model (functor.system ..the_type ..the_functor)
           (list ..identity_of_functor
                 ..composition_of_functor)))
