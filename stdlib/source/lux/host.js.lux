(.module:
  [lux #*
   [control
    ["p" parser
     ["s" code (#+ Parser)]]]
   [data
    [collection
     ["." list ("#@." fold)]]]
   [type
    abstract]
   [macro (#+ with-gensyms)
    [syntax (#+ syntax:)]
    ["." code]]])

(template [<name> <type>]
  [(abstract: #export <name> {} Any)]

  [Object]
  [Function]
  [Symbol]
  [Null]
  [Undefined]
  )

(template [<name> <type>]
  [(type: #export <name> <type>)]

  [String  Text]
  [Number  Frac]
  [Boolean Bit]
  )
