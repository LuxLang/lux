(.module:
  [lux #*
   [abstract
    [equivalence (#+ Equivalence)]]
   [data
    [format
     [binary (#+ Writer)]]]]
  ["." /// #_
   [constant (#+ Value)]
   ["#." index (#+ Index)]
   [encoding
    ["#." unsigned (#+ U2 U4)]]])

(type: #export (Constant a)
  (Index (Value a)))

(def: #export equivalence
  (All [a] (Equivalence (Constant a)))
