(.module:
  [lux #*
   [abstract
    [equivalence (#+ Equivalence)]]
   [data
    ["." format #_
     ["#" binary (#+ Writer)]]]]
  ["." /// #_
   [encoding
    ["#." signed (#+ S2 S4)]]])

(type: #export Jump S2)

(def: #export equivalence
  (Equivalence Jump)
  ///signed.equivalence)

(def: #export writer
  (Writer Jump)
  ///signed.writer/2)

(type: #export Big_Jump S4)

(def: #export lift
  (-> Jump Big_Jump)
  ///signed.lift/4)
