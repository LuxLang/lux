(;module:
  lux
  (lux/control [ord]))

## [Signatures]
(sig: #export (Number n)
  {#;doc "Everything that should be expected of a number type."}
  
  (: (ord;Ord n)
     ord)
  
  (do-template [<name>]
    [(: (-> n n n) <name>)]
    [+] [-] [*] [/] [%])

  (do-template [<name>]
    [(: (-> n n) <name>)]
    [negate] [signum] [abs])
  )
