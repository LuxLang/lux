(.module:
  lux)

(`` (signature: #export (Number n)
      {#.doc "Everything that should be expected of a number type."}

      (~~ (template [<name>]
            [(: (-> n n n) <name>)]
            [+] [-] [*] [/] [%]))

      (~~ (template [<name>]
            [(: (-> n n) <name>)]
            [negate] [signum] [abs]))
      ))
