(;module: lux)

(sig: #export (Functor f)
  (: (All [a b]
       (-> (-> a b) (f a) (f b)))
     map))

(def: #export (compF Functor<F> Functor<G>)
  (All [F G] (-> (Functor F) (Functor G) (Functor (All [a] (F (G a))))))
  (struct (def: (map f fga)
            (:: Functor<F> map (:: Functor<G> map f) fga))))
