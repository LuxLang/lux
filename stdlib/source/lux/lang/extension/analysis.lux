(.module:
  lux
  (lux (data [text]
             (coll [list "list/" Functor<List>]
                   (dictionary ["dict" unordered #+ Dict]))))
  [//]
  [/common]
  [/host])

(def: #export defaults
  (//.Extension //.Analysis)
  (|> /common.extensions
      (dict.merge /host.extensions)
      dict.entries
      (list/map (function (_ [name proc]) [name (proc name)]))
      (dict.from-list text.Hash<Text>)))
