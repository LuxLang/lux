(.module:
  [lux (#- Module)
   [data
    [collection
     [set (#+ Set)]]]
   [world
    [file (#+ Path)]]]
  [//
   [artifact (#+ Registry)]])

(type: #export Module Text)

(type: #export Descriptor
  {#name Module
   #file Path
   #hash Nat
   #state Module-State
   #references (Set Module)
   #registry Registry})
