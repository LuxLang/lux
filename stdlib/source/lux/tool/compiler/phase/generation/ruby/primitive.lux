(.module:
  [lux (#- i64)
   [control
    [pipe (#+ cond> new>)]]
   [data
    [number
     ["." frac]]]
   [host
    ["_" ruby (#+ Expression)]]]
  ["." // #_
   ["#." runtime]])

(def: #export bit
  (-> Bit (Expression Any))
  _.bool)

(def: #export i64
  (-> (I64 Any) (Expression Any))
  (|>> .int _.int))

(def: #export f64
  (-> Frac (Expression Any))
  _.float)

(def: #export text
  (-> Text (Expression Any))
  _.string)
