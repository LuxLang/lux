(.module:
  [lux (#- static)
   [abstract
    [equivalence (#+)]
    [monoid (#+)]]
   [control
    [parser (#+)]]
   [data
    [number (#+)
     [i64 (#+)]]
    [format
     [binary (#+)]]]
   [type
    [abstract (#+)]]]
  [// (#+ modifiers:)
   [//
    [encoding (#+)]]])

(modifiers:
  ["0001" public]
  ["0002" private]
  ["0004" protected]
  ["0008" static]
  ["0010" final]
  ["0200" interface]
  ["0400" abstract]
  ["1000" synthetic]
  ["2000" annotation]
  ["4000" enum]
  )
