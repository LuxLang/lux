(.module:
  [lux (#- static)
   [control
    [monoid (#+)]
    [parser (#+)]]
   [data
    [number (#+)
     [i64 (#+)]]
    [format
     [binary (#+)]]]
   [type
    [abstract (#+)]]]
  [// (#+ modifiers:)
   [//
    [encoding (#+)]]])

(modifiers:
  ["0001" public]
  ["0002" private]
  ["0004" protected]
  ["0008" static]
  ["0010" final]
  ["0020" synchronized]
  ["0040" bridge]
  ["0080" var-args]
  ["0100" native]
  ["0400" abstract]
  ["0800" strict]
  ["1000" synthetic]
  )
