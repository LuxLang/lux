(.module:
  [lux #*
   [control
    [monoid (#+)]
    [parser (#+)]]
   [data
    [number (#+)
     [i64 (#+)]]
    [format
     [binary (#+)]]]
   [type
    [abstract (#+)]]]
  [// (#+ modifiers:)
   [//
    [encoding (#+)]]])

(modifiers:
  ["0001" public]
  ["0010" final]
  ["0020" super]
  ["0200" interface]
  ["0400" abstract]
  ["1000" synthetic]
  ["2000" annotation]
  ["4000" enum]
  )
