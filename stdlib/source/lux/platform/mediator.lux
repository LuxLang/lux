(.module:
  [lux (#- Source Module)
   [data
    ["." error (#+ Error)]]
   [world
    ["." binary (#+ Binary)]
    ["." file (#+ File)]]]
  [//
   [compiler (#+ Compiler)
    [meta
     ["." archive (#+ Archive)
      [descriptor (#+ Module)]]]]])

(type: #export Source File)

(type: #export (Mediator !)
  (-> Archive Module (! Archive)))

(type: #export (Instancer ! d o)
  (-> (file.System !) (List Source) (Compiler d o) (Mediator !)))
