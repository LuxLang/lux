(.module:
  [lux #*
   ["_" test (#+ Test)]]
  ["." / #_
   ["#." version]
   ["#." pom]

   ["#." clean]
   ["#." install]

   ["#." deps]
   ["#." deploy]

   ["#." build]
   ["#." test]]
  {#program
   ["." /
    ## ["#." auto]
    ]})

(def: #export test
  Test
  (<| (_.covering /._)
      (_.for [/.Command])
      ($_ _.and
          /version.test
          /pom.test
          
          /clean.test
          /install.test

          /deps.test
          /deploy.test

          /build.test
          /test.test
          ## /auto.test
          )))
