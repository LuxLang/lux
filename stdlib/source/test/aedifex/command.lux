(.using
 [library
  [lux "*"
   ["_" test {"+" Test}]]]
 ["[0]" / "_"
  ["[1][0]" version]
  ["[1][0]" pom]

  ["[1][0]" clean]
  ["[1][0]" install]

  ["[1][0]" deps]
  ["[1][0]" deploy]

  ["[1][0]" build]
  ["[1][0]" test]
  ["[1][0]" auto]]
 [\\program
  ["[0]" /]])

(def: .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Command])
      (all _.and
           /version.test
           /pom.test
           
           /clean.test
           /install.test

           /deps.test
           /deploy.test

           /build.test
           /test.test
           /auto.test
           )))
