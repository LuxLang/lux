(.module:
  [lux #*
   ["_" test (#+ Test)]
   [abstract
    [monad (#+ do)]]
   [data
    ["." text]]
   [math
    ["." random (#+ Random)]]]
  [//
   ["@." artifact]]
  [\\program
   ["." /
    ["/#" // #_
     ["#." artifact]]]])

(def: #export test
  Test
  (<| (_.covering /._)
      (do {! random.monad}
        [sample @artifact.random]
        ($_ _.and
            (_.cover [/.repository /.uri]
                     (text.starts_with? /.repository
                                        (/.uri (get@ #//artifact.version sample) sample)))
            ))))
