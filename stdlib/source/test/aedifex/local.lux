(.using
 [library
  [lux (.full)
   ["_" test (.only Test)]
   [abstract
    [monad (.only do)]]
   [data
    ["[0]" text]]
   [math
    ["[0]" random (.only Random)]]]]
 [//
  ["@[0]" artifact]]
 [\\program
  ["[0]" /
   ["/[1]" // "_"
    ["[1][0]" artifact]]]])

(def: .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [sample @artifact.random]
        (all _.and
             (_.coverage [/.repository /.uri]
               (text.starts_with? /.repository
                                  (/.uri (the //artifact.#version sample) sample)))
             ))))
