(.require
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [data
    ["[0]" text]]
   [math
    ["[0]" random (.only Random)]]
   [test
    ["_" property (.only Test)]]]]
 [//
  ["@[0]" artifact]]
 [\\program
  ["[0]" / (.only)
   ["/[1]" //
    ["[1][0]" artifact]]]])

(def .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [sample @artifact.random]
        (all _.and
             (_.coverage [/.repository /.uri]
               (text.starts_with? /.repository
                                  (/.uri (the //artifact.#version sample) sample)))
             ))))
