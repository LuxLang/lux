(.module:
  [library
   [lux "*"
    ["_" test {"+" [Test]}]
    [abstract
     [\\specification
      ["$." equivalence]]]
    [math
     ["." random {"+" [Random]}]]]]
  [\\program
   ["." /]])

(def: .public random
  (Random /.Identity)
  ($_ random.and
      (random.ascii/alpha 10)
      (random.ascii/alpha 10)
      ))

(def: .public test
  Test
  (<| (_.covering /._)
      (_.for [/.User /.Password /.Identity]
             ($_ _.and
                 (_.for [/.equivalence]
                        ($equivalence.spec /.equivalence ..random))
                 ))))
