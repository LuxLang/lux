(.module:
  [lux #*
   ["_" test (#+ Test)]
   [abstract
    [\spec
     ["$." equivalence]]]
   [math
    ["." random (#+ Random)]]]
  [\\program
   ["." /]])

(def: #export random
  (Random /.Identity)
  ($_ random.and
      (random.ascii/alpha 10)
      (random.ascii/alpha 10)
      ))

(def: #export test
  Test
  (<| (_.covering /._)
      (_.for [/.Identity]
             ($_ _.and
                 (_.for [/.equivalence]
                        ($equivalence.spec /.equivalence ..random))
                 ))))
