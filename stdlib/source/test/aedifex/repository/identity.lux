(.using
 [library
  [lux (.full)
   ["_" test (.only Test)]
   [abstract
    [\\specification
     ["$[0]" equivalence]]]
   [math
    ["[0]" random (.only Random)]]]]
 [\\program
  ["[0]" /]])

(def: .public random
  (Random /.Identity)
  (all random.and
       (random.alphabetic 10)
       (random.alphabetic 10)
       ))

(def: .public test
  Test
  (<| (_.covering /._)
      (_.for [/.User /.Password /.Identity]
             (all _.and
                  (_.for [/.equivalence]
                         ($equivalence.spec /.equivalence ..random))
                  ))))
