(.module:
  [lux #*
   ["_" test (#+ Test)]
   [abstract
    {[0 #spec]
     [/
      ["$." equivalence]]}]
   [math
    ["." random (#+ Random)]]]
  {#program
   ["." /]})

(def: #export random
  (Random /.Origin)
  ($_ random.or
      (random.ascii/alpha 10)
      (random.ascii/alpha 10)
      ))

(def: #export test
  Test
  (<| (_.covering /._)
      (_.for [/.Origin]
             ($_ _.and
                 (_.for [/.equivalence]
                        ($equivalence.spec /.equivalence ..random))
                 ))))
