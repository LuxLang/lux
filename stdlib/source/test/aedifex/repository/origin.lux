(.require
 [library
  [lux (.except)
   ["_" test (.only Test)]
   [abstract
    [\\specification
     ["$[0]" equivalence]]]
   [math
    ["[0]" random (.only Random)]]]]
 [\\program
  ["[0]" /]])

(def .public random
  (Random /.Origin)
  (all random.or
       (random.alphabetic 10)
       (random.alphabetic 10)
       ))

(def .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Origin]
             (all _.and
                  (_.for [/.equivalence]
                         ($equivalence.spec /.equivalence ..random))
                  ))))
