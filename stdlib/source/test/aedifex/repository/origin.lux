(.using
  [library
   [lux "*"
    ["_" test {"+" Test}]
    [abstract
     [\\specification
      ["$[0]" equivalence]]]
    [math
     ["[0]" random {"+" Random}]]]]
  [\\program
   ["[0]" /]])

(def: .public random
  (Random /.Origin)
  ($_ random.or
      (random.ascii/alpha 10)
      (random.ascii/alpha 10)
      ))

(def: .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Origin]
             ($_ _.and
                 (_.for [/.equivalence]
                        ($equivalence.spec /.equivalence ..random))
                 ))))
