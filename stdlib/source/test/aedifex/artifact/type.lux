(.module:
  [lux #*
   ["_" test (#+ Test)]
   [abstract
    [monad (#+ do)]]
   [data
    ["." text]
    [number
     ["n" nat]]
    [collection
     ["." set]
     ["." list]]]
   [math
    ["." random (#+ Random)]]]
  {#program
   ["." /]})

(def: #export test
  Test
  (<| (_.covering /._)
      (_.with-cover [/.Type]
        ($_ _.and
            (_.cover [/.lux-library /.jvm-library
                      /.pom /.md5 /.sha1]
                     (let [options (list /.lux-library /.jvm-library
                                         /.pom /.md5 /.sha1)
                           uniques (set.from-list text.hash options)]
                       (n.= (list.size options)
                            (set.size uniques))))
            ))))
