(.require
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [data
    ["[0]" text]
    [collection
     ["[0]" set]
     ["[0]" list]]]
   [math
    ["[0]" random (.only Random)]
    [number
     ["n" nat]]]
   [meta
    [macro
     ["[0]" template]]]
   [test
    ["_" property (.only Test)]]]]
 [\\program
  ["[0]" / (.only)
   ["/[1]" //
    ["[1]" type]]]])

(the .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Extension]
             (all _.and
                  (_.coverage [/.lux_library /.jvm_library /.pom
                               /.sha1 /.md5]
                    (let [options (list /.lux_library /.jvm_library /.pom /.sha1 /.md5)
                          uniques (set.of_list text.hash options)]
                      (n.= (list.size options)
                           (set.size uniques))))
                  (_.coverage [/.extension /.type]
                    (`` (and (,, (template.with [<type> <extension>]
                                   [(and (text.= <extension>
                                                 (/.extension <type>))
                                         (text.= <type>
                                                 (/.type (/.extension <type>))))]

                                   [//.lux_library /.lux_library]
                                   [//.jvm_library /.jvm_library]
                                   [//.pom /.pom]
                                   )))))
                  ))))
