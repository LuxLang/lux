(.module:
  [lux #*
   ["_" test (#+ Test)]
   [control
    [io (#+ io)]
    [parser
     [cli (#+ program:)]]]]
  ["." / #_
   ["#." profile]
   ["#." cli]
   ["#." parser]])

(def: test
  Test
  ($_ _.and
      /profile.test
      /cli.test
      /parser.test
      ))

(program: args
  (<| io
      _.run!
      (_.times 100)
      ..test))
