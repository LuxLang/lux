(.module:
  [lux #*
   ["_" test (#+ Test)]
   [control
    [io (#+ io)]
    [parser
     [cli (#+ program:)]]]]
  ["." / #_
   ["#." artifact]
   ["#." input]
   ["#." command #_
    ["#/." pom]
    ["#/." install]]
   ["#." local]
   ["#." cache]
   ["#." dependency]
   ["#." package]
   ["#." profile]
   ["#." project]
   ["#." cli]
   ["#." hash]
   ["#." parser]
   ["#." pom]])

(def: test
  Test
  ($_ _.and
      /artifact.test
      /input.test
      /command/pom.test
      /command/install.test
      /local.test
      /cache.test
      /dependency.test
      /package.test
      /profile.test
      /project.test
      /cli.test
      /hash.test
      /parser.test
      /pom.test
      ))

(program: args
  (<| io
      _.run!
      (_.times 100)
      ..test))
