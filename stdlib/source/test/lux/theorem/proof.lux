(.using
 [library
  [lux (.except)
   [abstract
    ["[0]" monad]]
   [math
    ["[0]" random]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" contradiction]
  ["[1][0]" induction]
  ["[1][0]" inference]])

(the .public test
  Test
  (<| (_.covering /._)
      (monad.let [! random.monad]
        [])
      (all _.and
           (_.coverage [/.=]
             true)

           /contradiction.test
           /induction.test
           /inference.test
           )))
