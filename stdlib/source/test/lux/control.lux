(.module:
  [lux #*
   ["_" test (#+ Test)]]
  [/
   ["/." exception]
   ["/." interval]
   ["/." pipe]])

(def: #export test
  Test
  ($_ _.and
      (<| (_.context "/exception Exception-handling.")
          /exception.test)
      (<| (_.context "/interval")
          /interval.test)
      (<| (_.context "/pipe")
          /pipe.test)))
