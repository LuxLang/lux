(.using
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]
    ["[0]" functor]]
   [function
    [poly
     ["[0]" type]]]
   [math
    ["[0]" random]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]])

(the .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [])
      (_.for [/.Effect])
      (all _.and
           (_.coverage [/.functor]
             true)
           (_.coverage [/.Or /.or]
             (and (alias? type.Sum /.Or)
                  (alias? functor.sum /.or)))
           )))
