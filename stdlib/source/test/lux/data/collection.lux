(.module:
  [lux #*
   ["_" test (#+ Test)]]
  ["." / #_
   ["#." array]
   ["#." bits]
   ["#." list]
   ["#." row]
   ["#." sequence]
   ["#." stack]
   ["#." dictionary
    ["#/." ordered]
    ["#/." plist]]
   ["#." queue
    ["#/." priority]]
   ["#." set
    ["#/." multi]
    ["#/." ordered]]
   ["#." tree
    ["#/." zipper]]])

(def: dictionary
  Test
  ($_ _.and
      /dictionary.test
      /dictionary/ordered.test
      /dictionary/plist.test
      ))

(def: queue
  Test
  ($_ _.and
      /queue.test
      /queue/priority.test
      ))

(def: set
  Test
  ($_ _.and
      /set.test
      /set/multi.test
      /set/ordered.test
      ))

(def: tree
  Test
  ($_ _.and
      /tree.test
      /tree/zipper.test
      ))

(def: #export test
  Test
  ($_ _.and
      /array.test
      /bits.test
      /list.test
      /row.test
      /sequence.test
      /stack.test
      ..dictionary
      ..queue
      ..set
      ..tree
      ))
