(.module:
  [library
   [lux "*"
    ["_" test {"+" Test}]]]
  ["[0]" / "_"
   ["[1][0]" array]
   ["[1][0]" bits]
   ["[1][0]" list]
   ["[1][0]" sequence]
   ["[1][0]" stream]
   ["[1][0]" stack]
   ["[1][0]" dictionary
    ["[1]/[0]" ordered]
    ["[1]/[0]" plist]]
   ["[1][0]" queue
    ["[1]/[0]" priority]]
   ["[1][0]" set
    ["[1]/[0]" multi]
    ["[1]/[0]" ordered]]
   ["[1][0]" tree
    ["[1]/[0]" finger]
    ["[1]/[0]" zipper]]])

(def: dictionary
  Test
  ($_ _.and
      /dictionary.test
      /dictionary/ordered.test
      /dictionary/plist.test
      ))

(def: queue
  Test
  ($_ _.and
      /queue.test
      /queue/priority.test
      ))

(def: set
  Test
  ($_ _.and
      /set.test
      /set/multi.test
      /set/ordered.test
      ))

(def: tree
  Test
  ($_ _.and
      /tree.test
      /tree/finger.test
      /tree/zipper.test
      ))

(def: .public test
  Test
  ($_ _.and
      /array.test
      /bits.test
      /list.test
      /sequence.test
      /stream.test
      /stack.test
      ..dictionary
      ..queue
      ..set
      ..tree
      ))
