(.using
 [library
  [lux (.except)
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" digit]
  ["[1][0]" base]
  ["[1][0]" signum]
  ["[0]" /08
   ["[1]/[0]" natural]
   ["[1]/[0]" integer]]
  ["[0]" /16
