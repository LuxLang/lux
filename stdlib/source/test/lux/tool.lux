(.using
 [library
  [lux "*"
   ["_" test {"+" Test}]]]
 ["[0]" / "_"
  [compiler
   ["[1][0]" arity]
   ["[1][0]" version]
   ["[1][0]" reference]
   [language
    [lux
     ... ["[1][0]" syntax]
     ["[1][0]" analysis "_"
      ["[1]/[0]" primitive]
      ["[1]/[0]" composite]]
     ... [phase
     ...  ["[1][0]" analysis]
     ...  ["[1][0]" synthesis]]
     ]]
   ["[1][0]" meta "_"
    ["[1]/[0]" archive "_"
     ["[1]/[0]" signature]]]
   ]])

(def: .public test
  Test
  ($_ _.and
      /arity.test
      /version.test
      /reference.test
      /analysis/primitive.test
      /analysis/composite.test
      /meta/archive/signature.test
      ... /syntax.test
      ... /analysis.test
      ... /synthesis.test
      ))
