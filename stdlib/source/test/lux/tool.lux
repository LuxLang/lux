(.using
 [library
  [lux "*"
   ["_" test {"+" Test}]]]
 ["[0]" / "_"
  [compiler
   ["[1][0]" arity]
   ["[1][0]" version]
   ["[1][0]" reference]
   ["[1][0]" phase]
   [language
    [lux
     ... ["[1][0]" syntax]
     ["[1][0]" analysis]
     ["[1][0]" phase "_"
      ["[1]/[0]" extension]
      ["[1]/[0]" analysis "_"
       ["[1]/[0]" simple]
       ["[1]/[0]" complex]
       ["[1]/[0]" reference]]
      ... ["[1]/[0]" synthesis]
      ]]]
   ["[1][0]" meta "_"
    ["[1]/[0]" archive]
    ["[1]/[0]" cli]]
   ]])

(def: .public test
  Test
  ($_ _.and
      /arity.test
      /version.test
      /reference.test
      /phase.test
      /analysis.test
      /meta/archive.test
      /meta/cli.test
      /phase/extension.test
      /phase/analysis/simple.test
      /phase/analysis/complex.test
      /phase/analysis/reference.test
      ... /syntax.test
      ... /synthesis.test
      ))
