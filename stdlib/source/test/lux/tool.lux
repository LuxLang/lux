(.using
 [library
  [lux "*"
   ["_" test {"+" Test}]]]
 ["[0]" / "_"
  [compiler
   ["[1][0]" arity]
   ["[1][0]" version]
   ["[1][0]" reference]
   ["[1][0]" phase]
   [language
    [lux
     ... ["[1][0]" syntax]
     ["[1][0]" analysis]
     ["[1][0]" phase "_"
      ["[1]/[0]" extension]
      ["[1]/[0]" analysis "_"
       ["[1]/[0]" simple]]
      ... ["[1]/[0]" synthesis]
      ]]]
   ["[1][0]" meta "_"
    ["[1]/[0]" archive "_"
     ["[1]/[0]" artifact]
     ["[1]/[0]" signature]
     ["[1]/[0]" key]
     ["[1]/[0]" document]]]
   ]])

(def: .public test
  Test
  ($_ _.and
      /arity.test
      /version.test
      /reference.test
      /phase.test
      /analysis.test
      /meta/archive/artifact.test
      /meta/archive/signature.test
      /meta/archive/key.test
      /meta/archive/document.test
      /phase/extension.test
      /phase/analysis/simple.test
      ... /syntax.test
      ... /synthesis.test
      ))
