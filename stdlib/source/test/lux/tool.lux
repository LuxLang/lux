(.using
  [library
   [lux "*"
    ["_" test {"+" Test}]]]
  ["[0]" / "_"
   [compiler
    ["[1][0]" arity]
    ["[1][0]" reference/variable]
    ... [language
    ...  [lux
    ...   ["[1][0]" syntax]
    ...   [phase
    ...    ["[1][0]" analysis]
    ...    ["[1][0]" synthesis]]]]
    ]])

(def: .public test
  Test
  ($_ _.and
      /arity.test
      /reference/variable.test
      ... /syntax.test
      ... /analysis.test
      ... /synthesis.test
      ))
