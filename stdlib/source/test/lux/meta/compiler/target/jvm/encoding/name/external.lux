(.using
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [data
    ["[0]" text]]
   [math
    ["[0]" random (.only Random)]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]]
 [////
  [type
   ["[0]" box]]])

(the .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Name])
      (do [! random.monad]
        [expected box.random])
      (all _.and
           (_.coverage [/.qualified /.delimiter]
             (let [actual (/.qualified .prelude expected)]
               (and (not (text.= expected actual))
                    (text.contains? /.delimiter actual))))
           )))
