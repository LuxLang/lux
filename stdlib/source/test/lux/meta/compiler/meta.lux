(.require
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [math
    ["[0]" random (.only Random)]
    [number
     ["n" nat]]]
   [meta
    ["[0]" version]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" io]
  ["[1][0]" archive]
  ["[1][0]" cli]
  ["[1][0]" export]
  ["[1][0]" import]
  ["[1][0]" context]
  ["[1][0]" cache]])

(the .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [])
      (all _.and
           (_.coverage [/.version]
             (not (n.= version.latest
                       /.version)))

           /io.test
           /archive.test
           /cli.test
           /export.test
           /import.test
           /context.test
           /cache.test
           )))
