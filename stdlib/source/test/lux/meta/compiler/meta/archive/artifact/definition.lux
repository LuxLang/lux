(.require
 [library
  [lux (.except)
   [abstract
    ["[0]" equivalence
     ["[1]T" \\test]]]
   [math
    ["[0]" random (.only Random) (.use "[1]#[0]" monad)]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]])

(the .public random
  (Random /.Definition)
  (all random.and
       (random.lower_cased 1)
       (random.maybe
        (all random.and
             random.nat
             random.nat
             random.nat
             ))
       ))

(the .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Definition])
      (all _.and
           (_.for [/.equivalence]
                  (equivalenceT.spec /.equivalence ..random))
           )))
