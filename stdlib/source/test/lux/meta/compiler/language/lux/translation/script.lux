(.using
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [data
    ["[0]" text]
    [collection
     ["[0]" stack]
     ["[0]" set]]]
   [math
    ["[0]" random (.only Random)]
    [number
     ["n" natural]]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]])

(the .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [])
      (all _.and
           (_.coverage [/.variant_lefts /.variant_right? /.variant_choice
                        /.i64_low /.i64_high]
             (let [options (stack /.variant_lefts /.variant_right? /.variant_choice
                                  /.i64_low /.i64_high)
                   uniques (set.of_stack text.hash options)]
               (n.= (stack.size options)
                    (set.size uniques))))
           )))
