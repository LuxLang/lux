(.require
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [math
    ["[0]" random (.only Random)]]
   [meta
    [compiler
     [target
      [jvm
       ["[0]" modifier]
       ["[0]" method]]]]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]])

(def .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [])
      (all _.and
           (_.coverage [/.modifier]
             (and (modifier.has? method.public /.modifier)
                  (modifier.has? method.strict /.modifier)))
           )))
