(.require
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [math
    ["[0]" random (.only Random)]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" count]
  ["[1][0]" foreign]
  ["[1][0]" partial]])

(the .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [])
      (all _.and
           (_.coverage [/.type /.get /.put
                        /.variable /.variables]
             true)

           /count.test
           /foreign.test
           /partial.test
           )))
