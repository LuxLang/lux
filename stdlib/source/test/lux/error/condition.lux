(.using
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [function
    [polymorphism
     ["[0]" subject]]]
   [math
    ["[0]" random (.only Random)]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]])

(the .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [])
      (_.for [/.Condition])
      (all _.and
           (_.coverage [/.the]
             (same? subject.the /.the))
           (_.coverage [/.let]
             (same? subject.let /.let))
           (_.coverage [/.value]
             (same? subject.value /.value))
           )))
