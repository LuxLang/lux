(.module:
  [library
   [lux "*"
    ["_" test {"+" Test}]]]
  [\\library
   ["[0]" /]]
  ["[0]" / "_"
   ["[1][0]" equivalence]
   ["[1][0]" functor]
   ["[1][0]" json]])

(def: .public test
  Test
  (<| (_.covering /._)
      (_.for [/.poly: /.code])
      ($_ _.and
          /equivalence.test
          /functor.test
          /json.test
          )))
