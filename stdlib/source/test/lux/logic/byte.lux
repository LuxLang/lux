(.using
 [library
  [lux (.except)
   [abstract
    ["[0]" monad]
    ["[0]" equality
     ["[1]T" \\test]]]
   [math
    ["[0]" random]]
   [test
    ["_" property (.only Test)]]]]
 ["/" \\library]
 ["!" \\theorem])

(the .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Byte
              /.:7 /.:6 /.:5 /.:4 /.:3 /.:2 /.:1 /.:0

              /.random])
      (all _.and
           (_.for [/.equality]
                  (equalityT.spec /.equality /.random))
           
           (_.by_proof [/.false]
                       [!.false])
           (_.by_proof [/.true]
                       [!.true])
           (_.by_proof [/.not]
                       [!.involution_of_not])
           (_.by_proof [/.xor]
                       [!.identity_of_xor
                        !.commutativity_of_xor
                        !.associativity_of_xor])
           (_.by_proof [/.or]
                       [!.identity_of_or
                        !.commutativity_of_or
                        !.associativity_of_or])
           (_.by_proof [/.and]
                       [!.identity_of_and
                        !.commutativity_of_and
                        !.associativity_of_and])
           )))
