(.using
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]
    ["[0]" equivalence
     ["[1]T" \\test]]]
   [math
    ["[0]" random]
    [number
     [/64
      ["[0]" natural]
      ["[0]" decimal]]]]
   [test
    ["_" property (.only Test)]]
   [meta
    ["[0]" static]]
   [logic
    ["[0]" bit]]
   [macro
    ["[0]" expansion]]
   [data
    [collection
     ["[0]" list]
     ["[0]" set]]]]]
 [\\library
  ["[0]" / (.only)
   ["[0]" //]]])

(the .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [])
      (all _.and
           (<| (_.for [//.Elasticity])
               (expansion.let [,*every_modulus (these /.tungsten
                                                      /.steel
                                                      /.copper
                                                      /.brass
                                                      /.aluminium
                                                      /.quartz
                                                      /.water
                                                      /.mercury)]
                 (_.coverage [,*every_modulus]
                   (let [every_option (list ,*every_modulus)
                         every_unique_option (set.of_list decimal.hash every_option)]
                     (natural.= (list.size every_option)
                                (set.size every_unique_option))))))
           )))
