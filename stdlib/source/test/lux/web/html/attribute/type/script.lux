(.using
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [data
    ["[0]" text]
    [collection
     ["[0]" list]
     ["[0]" set]]]
   [math
    ["[0]" random (.only Random)]
    [number
     [/64
      ["n" natural]]]]
   [macro
    ["[0]" expansion]
    ["[0]" template]]
   [world
    [net
     ["[0]" mime (.only)
      ["[1]T" \\test]]]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]])

(expansion.let [<tags> (template.with [<name>]
                         [<name>]

                         [[/.module]
                          [/.import_map]
                          [/.javascript]])]
  (the .public test
    Test
    (<| (_.covering /._)
        (do [! random.monad]
          [mime mimeT.random])
        (_.for [/.Type])
        (all _.and
             (_.coverage [<tags>]
               (let [all (is (List /.Type)
                             (list <tags>))
                     uniques (set.of_list text.hash all)]
                 (n.= (list.size all)
                      (set.size uniques))))
             (_.coverage [/.data]
               (same? (mime.name mime)
                      (/.data mime)))
             ))))
