                                )]
  (these (the all_options
           (List /.Role)
           (`` (`` (`` (list.together (list (,, (template.with [,options]
                                                  [(is (List /.Role)
                                                       (list (,, (expansion.let [,options' (template.spliced ,options)]
                                                                   (,, (template.with [,name]
                                                                         [,name]

                                                                         ,options'
                                                                         ))))))]
                                                  
                                                  ,bundles))))))))

         (the .public test
           Test
           (<| (_.covering /._)
               (do [! random.monad]
                 [])
               (_.for [/.Role])
               (`` (all _.and
                        (,, (template.with [,options]
                              [(`` (`` (_.coverage [(,, (expansion.let [,options' (template.spliced ,options)]
                                                          (,, (template.with [,name]
                                                                [,name]

                                                                ,options'
                                                                ))))]
                                         (let [uniques (|> ..all_options
                                                           (set.of_list text.hash))]
                                           (n.= (list.size ..all_options)
                                                (set.size uniques))))))]
                              
                              ,bundles))
                        ))))))
