                                   [/.telnet/tls]
                                   [/.internet_message_access_protocol_secure]
                                   [/.post_office_protocol_3_secure])]
  (these (the .public random
           (Random /.Port)
           (`` (all random.either
                    (,, (template.with [<port>]
                          [(random#in <port>)]

                          <options/0>
                          <options/1>
                          <options/2>
                          <options/3>
                          )))))

         (the options
           (List /.Port)
           (list.together (list <all_options>)))
         
         (the .public test
           Test
           (<| (_.covering /._)
               (do [! random.monad]
                 [])
               (_.for [/.Port])
               (`` (all _.and
                        <coverages>
                        (,, (template.with [<long> <short>]
                              [(_.coverage [<short>]
                                 (same? <long> <short>))]

