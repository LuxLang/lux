  (def .public random
    (Random /.Port)
    (`` (all random.either
             (,, (with_template [<port>]
                   [(random#in <port>)]

                   <options>
                   )))))
  
  (def .public test
    Test
    (<| (_.covering /._)
        (do [! random.monad]
          [])
        (_.for [/.Port])
        (`` (all _.and
                 (_.coverage [(,, (with_template [<port>]
                                    [<port>]

                                    <options>
                                    ))]
                   (let [options (list <options>)
                         uniques (set.of_list nat.hash options)]
                     (nat.= (list.size options)
                            (set.size uniques))))
                 (,, (with_template [<long> <short>]
                       [(_.coverage [<short>]
                          (same? <long> <short>))]

