(.module:
  [library
   [lux "*"
    ["_" test {"+" [Test]}]
    [abstract
     [monad {"+" [do]}]]
    [control
     ["[0]" io]]
    [math
     ["[0]" random]]]]
  ["[0]" / "_"
   ["[1][0]" watch]]
  [\\library
   ["[0]" /]]
  [\\specification
   ["$[0]" /]])

(def: .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [/ (random.ascii/upper 1)]
        ($_ _.and
            (_.for [/.mock]
                   ($/.spec (io.io (/.mock /))))
            
            /watch.test
            ))))
