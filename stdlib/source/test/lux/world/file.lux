(.module:
  [library
   [lux #*
    ["_" test (#+ Test)]
    [abstract
     [monad (#+ do)]]
    [control
     ["." io]]
    [math
     ["." random]]]]
  ["." / #_
   ["#." watch]]
  [\\library
   ["." /]]
  [\\spec
   ["$." /]])

(def: #export test
  Test
  (<| (_.covering /._)
      (do {! random.monad}
        [/ (random.ascii/upper 1)]
        ($_ _.and
            (_.for [/.mock]
                   ($/.spec (io.io (/.mock /))))
            
            /watch.test
            ))))
