(.require
 [library
  [lux (.except)
   [abstract
    [monad (.only do)]]
   [math
    ["[0]" random (.only Random)]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" mime]
  ["[1][0]" uri]
  ["[1][0]" http]])

(def .public test
  Test
  (<| (_.covering /._)
      (do [! random.monad]
        [])
      (all _.and
           (_.coverage [/.Host]
             true)
           (_.coverage [/.URL]
             true)
           (_.coverage [/.Address
                        /.#host /.#port]
             true)

           /mime.test
           /http.test
           /uri.test
           )))
