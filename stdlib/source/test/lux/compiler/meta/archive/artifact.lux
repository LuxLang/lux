(.using
 [library
  [lux (.except)
   [abstract
    ["[0]" equivalence
     ["[1]T" \\test]]]
   [math
    ["[0]" random (.only Random)]]
   [meta
    ["[0]" static]]
   [test
    ["_" property (.only Test)]]]]
 ["[0]" /
  ["[1][0]" definition]
  ["[1][0]" category]]
 [\\library
  ["[0]" /]])

(the .public random
  (Random /.Artifact)
  (all random.and
       random.natural
       /category.random
       random.bit
       ))

(the .public test
  Test
  (<| (_.covering /._)
      (_.for [/.ID

              /.Artifact
              /.#id /.#category /.#mandatory?])
      (all _.and
           (_.for [/.equivalence /.=]
                  (static.when (same? /.equivalence /.=)
                    (equivalenceT.spec /.equivalence ..random)))

           /definition.test
           /category.test
           )))
