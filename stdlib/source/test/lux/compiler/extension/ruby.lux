(.using
 [library
  [lux (.except)
   [abstract
    ["[0]" monad]]
   [data
    ["[0]" text]
    [collection
     ["[0]" list]
     ["[0]" set]]]
   [macro
    ["[0]" expansion]]
   [math
    ["[0]" random]
    [number
     [/64
      ["n" natural]]]]
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]])

(the .public test
  Test
  (<| (_.covering /._)
      (monad.let [! random.monad]
        [])
      (all _.and
           (expansion.let [,*options (these /.object_get
                                            /.object_do
                                            /.object_nil
                                            /.object_nil?

                                            /.constant
                                            /.of
                                            /.import)]
             (_.coverage [,*options]
               (let [options (list ,*options)
                     uniques (set.of_list text.hash options)]
                 (n.= (list.size options)
                      (set.size uniques)))))
           )))
