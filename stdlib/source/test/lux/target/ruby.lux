(def: test|computation
  Test
  (do [! random.monad]
    [test random.bit
     then random.safe_frac
     else random.safe_frac

     bool random.bit
     float random.frac
     string (random.ascii/upper 5)

     comment (random.ascii/upper 10)]
    ($_ _.and
        ..test|bool
        ..test|float
        ..test|int
        ..test|array
        ..test|hash
        ... ..test|object
        (_.cover [/.?]
                 (let [expected (if test then else)]
                   (expression (|>> (:as Frac) (f.= expected))
                               (/.? (/.bool test)
                                    (/.float then)
                                    (/.float else)))))
        (_.cover [/.comment]
                 (expression (|>> (:as Frac) (f.= then))
                             (/.comment comment
                               (/.float then))))
        )))

