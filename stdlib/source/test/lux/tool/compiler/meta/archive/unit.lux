(.using
 [library
  [lux "*"
   ["_" test {"+" Test}]
   [abstract
    [\\specification
     ["$[0]" equivalence]
     ["$[0]" hash]]]
   [data
    [collection
     ["[0]" set]]]
   [math
    ["[0]" random {"+" Random}]]]]
 [\\library
  ["[0]" /]])

(def: .public random
  (Random /.ID)
  (all random.and
       random.nat
       random.nat
       ))

(def: .public test
  Test
  (<| (_.covering /._)
      (_.for [/.ID])
      (all _.and
           (_.for [/.equivalence]
                  ($equivalence.spec /.equivalence ..random))
           (_.for [/.hash]
                  ($hash.spec /.hash ..random))
           
           (_.cover [/.none]
                    (set.empty? /.none))
           )))
