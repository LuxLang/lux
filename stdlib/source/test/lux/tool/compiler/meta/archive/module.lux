(.using
 [library
  [lux "*"
   ["_" test {"+" Test}]
   [abstract
    [monad {"+" do}]]
   [math
    ["[0]" random {"+" Random}]
    [number
     ["n" nat]]]]]
 [\\library
  ["[0]" /
   ["[0]" document]
   [//
    ["[0]" key]
    ["[0]" signature "_"
     ["$[1]" \\test]]]]]
 ["[0]" / "_"
  ["[1][0]" document]
  ["[1][0]" descriptor]])

(def: .public (random it)
  (All (_ a) (-> (Random a) (Random (/.Module a))))
  (all random.and
       random.nat
       (/descriptor.random 0)
       (do random.monad
         [signature $signature.random
          example it]
         (in (document.document (key.key signature example)
                                example)))))

(def: .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Module])
      (all _.and
           (_.coverage [/.ID /.runtime]
             (n.= 0 /.runtime))

           /document.test
           /descriptor.test
           )))
