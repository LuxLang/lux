(.using
 [library
  [lux "*"
   ["_" test {"+" Test}]
   [abstract
    [monad {"+" do}]]
   [data
    ["[0]" bit ("[1]#[0]" equivalence)]
    ["[0]" text ("[1]#[0]" equivalence)
     ["%" format {"+" format}]]]
   [math
    ["[0]" random {"+" Random}]
    [number
     ["n" nat]]]]]
 [\\library
  ["[0]" /]])

(def: .public random
  (Random /.Version)
  random.nat)

(def: .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Version])
      (do [! random.monad]
        [this ..random
         that ..random]
        (`` (all _.and
                 (_.cover [/.format]
                          (bit#= (n.= this that)
                                 (text#= (/.format this) (/.format that))))
                 (~~ (template [<level>]
                       [(_.cover [<level>]
                                 (text.contains? (%.nat (<level> this))
                                                 (/.format this)))]

                       [/.patch]
                       [/.minor]
                       [/.major]))
                 )))))
