(.module:
  [lux #*
   data/text/format
   ["_" test (#+ Test)]
   [control
    {[0 #test]
     [/
      ["$." equivalence]
      ["$." order]
      ["$." codec]]}]
   [math
    ["r" random (#+ Random)]]
   [time
    ["@." instant]]]
  [//
   ["_." instant]]
  {1
   ["." / (#+ Date)]})

(def: #export date
  (Random Date)
  (|> _instant.instant (:: r.monad map @instant.date)))

(def: #export test
  Test
  ($_ _.and
      ($equivalence.spec /.equivalence ..date)
      ($order.spec /.order ..date)
      (<| (_.seed 6623983470548808292)
          ($codec.spec /.equivalence /.codec ..date))
      ))
