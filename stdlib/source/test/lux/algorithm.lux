(.require
 [library
  [lux (.except)
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" mix]
  ["[1][0]" size]
  ["[1][0]" any?]
  ["[1][0]" every?]
  ["[1][0]" any]
  ["[1][0]" item]])

(the .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Constant /.Variable /.Recursive
              /.Sum /.Product])
      (all _.and
           /mix.test
           /size.test
           /any?.test
           /every?.test
           /any.test
           /item.test
           )))
