(.require
 [library
  [lux (.except)
   [test
    ["_" property (.only Test)]]]]
 [\\library
  ["[0]" /]]
 ["[0]" /
  ["[1][0]" mix]
  ["[1][0]" size]
  ["[1][0]" any?]])

(the .public test
  Test
  (<| (_.covering /._)
      (_.for [/.Constant /.Variable
              /.Sum /.Product
              /.Recursive])
      (all _.and
           /mix.test
           /size.test
           /any?.test
           )))
