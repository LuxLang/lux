(.using
 [library
  [lux (.except)
   [abstract
    [monad (.only Monad)]]
   [concurrency
    ["[0]" async (.only Async)]]
   [control
    ["[0]" try (.only Try)]]]])

(every .public (Action of)
  (Async (Try of)))

(the .public monad
  (Monad Action)
  (as_expected (try.with async.monad)))
