(.require
 [library
  [lux (.except #Local)
   [abstract
    [equivalence (.only Equivalence)]]
   [data
    ["[0]" sum]
    ["[0]" text]]
   [world
    [file (.only Path)]
    [net (.only URL)]]]])

(type .public Origin
  (Variant
   {#Local Path}
   {#Remote URL}))

(def .public equivalence
  (Equivalence Origin)
  (all sum.equivalence
       text.equivalence
       text.equivalence
       ))
