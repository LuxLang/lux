(.module:
  [lux (#- Type)
   [abstract
    ["." equivalence (#+ Equivalence)]
    ["." hash (#+ Hash)]]
   [data
    ["." text]]
   [world
    [net (#+ URL)]]]
  ["." // #_
   ["#." artifact (#+ Artifact)
    [type (#+ Type)]]])

(type: #export Repository
  URL)

(type: #export Dependency
  {#artifact Artifact
   #type Type})

(def: #export equivalence
  (Equivalence Dependency)
  ($_ equivalence.product
      //artifact.equivalence
      text.equivalence
      ))

(def: #export hash
  (Hash Dependency)
  ($_ hash.product
      //artifact.hash
      text.hash
      ))
