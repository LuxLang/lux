(.module:
  [lux #*
   [data
    [text
     ["%" format (#+ format)]]]
   [world
    [net
     ["." uri (#+ URI)]]]]
  ["." // #_
   ["#." artifact (#+ Version Artifact)]])

(def: / uri.separator)

(def: #export repository
  URI
  (format ".m2" / "repository"))

(def: #export (uri version artifact)
  (-> Version Artifact URI)
  (format ..repository / (//artifact.uri version artifact)))
