(.module:
  [lux #*
   [data
    [text
     ["%" format (#+ format)]]]
   [world
    [net
     ["." uri (#+ URI)]]]]
  ["." // #_
   ["#." artifact (#+ Artifact)]])

(def: / uri.separator)

(def: #export repository
  URI
  (format ".m2" / "repository"))

(def: #export uri
  (-> Artifact URI)
  (|>> //artifact.uri
       (format ..repository /)))
