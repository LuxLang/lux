(.require
 [library
  [lux (.except)
   [data
    [text
     ["%" \\format (.only format)]]]
   [world
    [net
     [uri (.only URI)
      ["[0]" path]]]]]]
 ["[0]" //
  ["[1][0]" artifact (.only Version Artifact)]])

(the / path.separator)

(the .public repository
  URI
  (format ".m2" / "repository"))

(the .public (uri version artifact)
  (-> Version Artifact URI)
  (format ..repository / (//artifact.uri version artifact)))
