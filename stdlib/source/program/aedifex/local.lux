(.using
 [library
  [lux "*"
   [data
    [text
     ["%" format (.only format)]]]
   [world
    [net
     ["[0]" uri (.only URI)]]]]]
 ["[0]" // "_"
  ["[1][0]" artifact (.only Version Artifact)]])

(def: / uri.separator)

(def: .public repository
  URI
  (format ".m2" / "repository"))

(def: .public (uri version artifact)
  (-> Version Artifact URI)
  (format ..repository / (//artifact.uri version artifact)))
