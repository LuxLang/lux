(.module:
  [library
   [lux "*"
    [control
     [concurrency
      ["[0]" async {"+" Async}]]]
    [tool
     [compiler
      ["[0]" version]
      ["[0]" language "_"
       ["[1]/[0]" lux "_"
        ["[1]" version]]]]]
    [world
     ["[0]" console {"+" Console}]]]]
  [///
   [command {"+" Command}]])

(def: .public (do! console profile)
  (-> (Console Async) (Command Any))
  (console.write_line (version.format language/lux.version)
                      console))
