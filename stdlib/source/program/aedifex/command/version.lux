(.using
 [library
  [lux "*"
   [control
    [try {"+" Try}]
    [concurrency
     ["[0]" async {"+" Async}]]]
   ["[0]" meta "_"
    ["[1]/[0]" version]]
   [tool
    [compiler
     ["[0]" version]]]
   [world
    ["[0]" console {"+" Console}]]]]
 [///
  [command {"+" Command}]])

(def: .public (do! console profile)
  (-> (Console Async) (Command Any))
  (is (Async (Try Any))
      (console.write_line (version.format meta/version.latest)
                          console)))
