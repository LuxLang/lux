(.using
 [library
  [lux (.except)
   [concurrency
    ["[0]" async (.only Async)]]
   [control
    [try (.only Try)]]
   ["[0]" meta
    ["[1]/[0]" version]
    [compiler
     ["[0]" version]]]
   [world
    ["[0]" console (.only Console)]]]]
 [///
  [command (.only Command)]])

(the .public (do! console profile)
  (-> (Console Async) (Command Any))
  (is (Async (Try Any))
      (console.write_line (version.injection meta/version.latest)
                          console)))
