(.require
 [library
  [lux (.except)
   [control
    [try (.only Try)]
    [concurrency
     ["[0]" async (.only Async)]]]
   ["[0]" meta
    ["[1]/[0]" version]]
   [tool
    [compiler
     ["[0]" version]]]
   [world
    ["[0]" console (.only Console)]]]]
 [///
  [command (.only Command)]])

(def .public (do! console profile)
  (-> (Console Async) (Command Any))
  (is (Async (Try Any))
      (console.write_line (version.format meta/version.latest)
                          console)))
