(.module:
  [library
   [lux #*
    [control
     [concurrency
      ["." async (#+ Async)]]]
    [tool
     [compiler
      ["." version]
      ["." language #_
       ["#/." lux #_
        ["#" version]]]]]
    [world
     ["." console (#+ Console)]]]]
  [///
   [command (#+ Command)]])

(def: .public (do! console profile)
  (-> (Console Async) (Command Any))
  (console.write_line (version.format language/lux.version)
                      console))
