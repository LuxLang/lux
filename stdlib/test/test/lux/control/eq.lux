(.module:
  lux
  (lux (control ["/" eq]
                [monad #+ do])
       (math ["r" random])
       test))

(def: #export (spec Eq<a> gen<a>)
  (All [a] (-> (/.Eq a) (r.Random a) Test))
  (do r.Monad<Random>
    [sample gen<a>]
    (test "Equality is reflexive."
          (:: Eq<a> = sample sample))))
