(.module:
  [lux #*
   [data
    ["@" product]]]
  lux/test)

(context: "Products"
  ($_ seq
      (test "Can access the sides of a pair."
            (and (i/= 1 (@.left [1 2]))
                 (i/= 2 (@.right [1 2]))))
      
      (test "Can swap the sides of a pair."
            (let [[_left _right] (@.swap [1 2])]
              (and (i/= 2 _left)
                   (i/= 1 _right))))
      ))
