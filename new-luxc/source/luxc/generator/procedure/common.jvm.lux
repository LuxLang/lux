     (|>. inputI ($i;unwrap #$;Double)
          paramI ($i;unwrap #$;Double)
          ($i;INVOKESTATIC "java.lang.Math" <method> math-binary-method false)
          ($i;wrap #$;Double)))]

  [math//atan2 "atan2"]
