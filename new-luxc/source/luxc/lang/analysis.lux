(;module:
  lux
  (lux (data [product]))
  (.. ["lp" pattern]))

(type: #export #rec Analysis
  #Unit
  (#Bool Bool)
  (#Nat Nat)
  (#Int Int)
  (#Deg Deg)
  (#Real Real)
  (#Char Char)
  (#Text Text)
  (#Sum (Either Analysis Analysis))
  (#Product Analysis Analysis)
  (#Case Analysis (List [lp;Pattern Analysis]))
  (#Function Scope Analysis)
  (#Apply Analysis Analysis)
  (#Procedure Text (List Analysis))
  (#Relative Ref)
  (#Absolute Ident))
