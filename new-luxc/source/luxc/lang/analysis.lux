(;module:
  lux
  (lux (data [product]))
  (.. ["lp" pattern]))

(type: #export #rec Analysis
  (#Bool Bool)
  (#Nat Nat)
  (#Int Int)
  (#Deg Deg)
  (#Real Real)
  (#Char Char)
  (#Text Text)
  (#Variant Nat Nat Analysis)
  (#Tuple (List Analysis))
  (#Case Analysis (List [lp;Pattern Analysis]))
  (#Function Scope Analysis)
  (#Apply Analysis Analysis)
  (#Procedure Ident (List Analysis))
  (#Relative Ref)
  (#Absolute Ident))
