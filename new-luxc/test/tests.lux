(;module:
  lux
  (lux (control monad)
       [io]
       (concurrency [promise])
       [cli #+ program:]
       [test])
  (test (luxc ["_;" parser]
              (analyser ["_;" primitive]
                        ["_;" structure]
                        ["_;" reference]
                        ["_;" case]
                        ["_;" function]
                        (procedure ["_;" common])
                        ))))

## [Program]
(program: args
  (test;run))
